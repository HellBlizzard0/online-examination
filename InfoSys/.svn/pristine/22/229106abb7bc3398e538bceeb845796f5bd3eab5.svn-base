<h:head></h:head>
<h:body class="fixed-top">
	<ui:composition>
		<script type="text/javascript">
        	function maskScreen(){
            	PF('reload').show();
            }
        	
        	function unMaskScreen(){
        		PF('reload').hide();
            }
       	</script>
		<meta name="viewport" content="width=device-width, initial-scale=1" />

		<prime:growl showDetail="true" autoUpdate="true" id="notifyMessagesId" widgetVar="notifyMessagesWVId" />
		<prime:ajaxStatus onstart="PF('reload').show();" onsuccess="PF('reload').hide();" />
		<prime:dialog id="reload" widgetVar="reload" modal="true" showHeader="false" resizable="false" closable="false">
			<h:graphicImage value="/resources/images/wait.png" alt="ai" />
		</prime:dialog>
		<div id="container" class="row-fluid">
			<div class="bg-Drark">
				<div id="bodyMiniSearch">
					<!-- BEGIN PAGE CONTAINER-->
					<ui:insert />
					<!-- END PAGE CONTAINER-->
				</div>
			</div>
		</div>
		<!-- BEGIN COPYRIGHT -->

		<div class="footer">
			<ui:include src="/Main/footer.xhtml" />
		</div>
	</ui:composition>
</h:body>
</html>