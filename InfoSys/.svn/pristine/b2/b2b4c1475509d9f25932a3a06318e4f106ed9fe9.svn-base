<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN"
  "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml" xmlns:h="http://java.sun.com/jsf/html" xmlns:p="http://primefaces.org/ui" xmlns:ui="http://java.sun.com/jsf/facelets" xmlns:f="http://java.sun.com/jsf/core" xmlns:util="http://java.sun.com/jsf/composite/util">
<h:head>
	<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
	<h:outputStylesheet library="css" name="styles.css" />
	<h:outputStylesheet library="css" name="grid.css" />
	<h:outputStylesheet library="css" name="bootstrap.css" />
	
	<h:outputScript name="utilities.js" library="javascript" target="head" />
	<script type="text/javascript">
	window.onkeypress = function(e) {
		  var keyCode = window.event.keyCode || e.keyCode || e.which;
	      if (keyCode == '13') {
	    	  document.getElementById('missionReportSearchFormId:searchBtn').click();
	    	  return false;
	      }
	};
    </script>
</h:head>
<h:body styleClass="screenBg">
	<h:form id="missionReportSearchFormId" dir="#{msgs.dir}">
		<ui:decorate template="/Main/Template.xhtml">
			<table border="0" cellpadding="0" cellspacing="0" width="100%">

				<tr>
					<td class="screenTitle">#{msgs.title_missionReportSearch}</td>
				</tr>

				<tr>
					<td width="100%" height="5" />
				</tr>

				<tr>
					<td colspan="2">
						<table class="sectionTitle" border="0" cellpadding="0" cellspacing="0">
							<tr>
								<td>
									<h:outputText value="#{msgs.label_searchData}" />
								</td>
							</tr>
						</table>
					</td>
				</tr>

				<tr>
					<td width="100%" height="5" />
				</tr>

				<tr>
					<td>
						<h:panelGroup id="searchDataPnlId">
							<table class="sectionContent" border="0" cellpadding="0" cellspacing="0">
								<tr>
									<th width="15%">#{msgs.label_missionStartDate} #{msgs.label_from}</th>
									<td width="20%">
										<util:advancedHijriDate id="missionStartDateId" value="#{missionReportsSearch.labCheckReportSearchData.startDate}" />
									</td>
									
									<th width="15%">#{msgs.label_missionEndDate} #{msgs.label_to}</th>
									<td width="20%">
										<util:advancedHijriDate id="missionEndDateId" value="#{missionReportsSearch.labCheckReportSearchData.endDate}" />
									</td>
									
								</tr>
								<tr>
									<th width="15%">#{msgs.label_missionType}</th>
									<td width="20%">
										<h:selectOneMenu style="" id="missionTypeId" styleClass="form-control" value="#{missionReportsSearch.labCheckReportSearchData.missionTypeId}">
											<f:selectItem itemLabel="" itemValue="" />
											<f:selectItems value="#{missionReportsSearch.labCheckMissionTypes}" var="type" itemLabel="#{type.description}" itemValue="#{type.id}" />
										</h:selectOneMenu>
									</td>
									
									<th width="15%">#{msgs.label_destinationDepartment}</th>
									<td width="20%">
										<h:selectOneMenu style="" id="destinationDepartmentId" styleClass="form-control" value="#{missionReportsSearch.labCheckReportSearchData.destinationDepartmentId}">
											<f:selectItem itemLabel="" itemValue="" />
											<f:selectItems value="#{missionReportRegisteration.regionList}" var="region" itemLabel="#{region.arabicName}" itemValue="#{region.id}" />
										</h:selectOneMenu>
									</td>
									
								</tr>
							</table>
						</h:panelGroup>
					</td>
				</tr>

				<tr>
					<td width="100%" height="5" />
				</tr>

				<tr>
					<td width="100%">
						<table class="actionsSection" border="0" cellpadding="0" cellspacing="0">
							<tr>
								<td>
									<h:commandButton styleClass="btn btn-default" id="resetBtn" value="&#xf021; #{msgs.label_reset}" action="#{missionReportsSearch.reset}">
										<f:ajax render="missionReportSearchFormId:notifyMessagesId missionReportSearchFormId:searchDataPnlId" />
									</h:commandButton>
									<h:commandButton styleClass="btn btn-default" id="searchBtn" value="&#xf002; #{msgs.label_search}" action="#{missionReportsSearch.search}">
										<f:ajax render="missionReportSearchFormId:notifyMessagesId missionReportSearchFormId:searchResultsId" execute="missionReportSearchFormId:searchDataPnlId" />
									</h:commandButton>
								</td>
							</tr>
						</table>
					</td>
				</tr>

				<tr>
					<td width="100%" height="5" />
				</tr>

				<tr>
					<td width="100%">
						<p:dataTable tableStyle="table-layout:auto" reflow="true" liveResize="true" paginatorPosition="bottom" id="searchResultsId" paginator="true" style="width:100%" rowIndexVar="index" rows="10" var="labCheckReportResult" value="#{missionReportsSearch.labCheckReportDataResultList}" widgetVar="table"
						rowHover="true">
							<p:column styleClass='centerText'>
								<f:facet name="header">
									#
								</f:facet>
									#{index + 1}
								</p:column>
							<p:column styleClass='centerText'>
								<f:facet name="header">
									#{msgs.label_labCheckOrderNumber}
								</f:facet>
								<h:outputText value="#{labCheckReportResult.orderNumber}" />
							</p:column>
							<p:column styleClass='centerText'>
								<f:facet name="header">
									#{msgs.label_missionType}
								</f:facet>
								<h:outputText value="#{labCheckReportResult.missionTypeDescription}" />
							</p:column>
							<p:column styleClass='centerText'>
								<f:facet name="header">
									#{msgs.label_missionPeriod}/#{msgs.label_day}
								</f:facet>
								<h:outputText value="#{labCheckReportResult.period}" />
							</p:column>
							<p:column styleClass='centerText'>
								<f:facet name="header"> 
									#{msgs.label_missionStartDate}
								</f:facet>
								<h:outputText value="#{labCheckReportResult.startDateString}" />
							</p:column>
							<p:column styleClass='centerText'>
								<f:facet name="header"> 
									#{msgs.label_missionEndDate}
								</f:facet>
								<h:outputText value="#{labCheckReportResult.endDateString}" />
							</p:column>
							<p:column styleClass='centerText'>
								<f:facet name="header">
									<h:outputText value="#{msgs.label_view}" />
								</f:facet>
								<h:commandButton value="&#xf06e;" styleClass="btn btn-trans-icon" action="#{missionReportsSearch.view(labCheckReportResult.id)}">
								</h:commandButton>
							</p:column>
							<p:column styleClass='centerText'>
								<f:facet name="header">
									<h:outputText value="#{msgs.label_delete}" />
								</f:facet>
								<h:commandButton value="&#xf00d;" styleClass="btn btn-trans-icon" action="#{missionReportsSearch.deleteMissionReport(labCheckReportResult)}" onclick="return confirm('#{msgs.q_delete}');">
									<f:ajax render="missionReportSearchFormId:notifyMessagesId missionReportSearchFormId:searchResultsId">
									</f:ajax>
								</h:commandButton>
							</p:column>
						</p:dataTable>
					</td>
				</tr>
			</table>
		</ui:decorate>
	</h:form>
</h:body>
</html>