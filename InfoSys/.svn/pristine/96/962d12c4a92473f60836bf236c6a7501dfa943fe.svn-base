 	  }

	 function getInfoDependsEmployee(contextPath,socialId){
		 openEmployeeInfoSysDetailsBySocialId(contextPath,socialId,1,'#{infoView.infoClosedDate}');
 		  return false;
 	  }

	 function releaseMask(){
 		unMaskScreen();
   }

     </script>
