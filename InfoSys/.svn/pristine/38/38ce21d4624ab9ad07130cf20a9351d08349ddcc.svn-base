<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN"
  "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml" xmlns:h="http://java.sun.com/jsf/html" xmlns:ui="http://java.sun.com/jsf/facelets" xmlns:f="http://java.sun.com/jsf/core" xmlns:util="http://java.sun.com/jsf/composite/util" xmlns:rich="http://richfaces.org/rich">
<h:head>
	<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
	<h:outputStylesheet library="css" name="styles.css" />
	<h:outputStylesheet library="css" name="bootstrap.css" />
	<h:outputScript name="utilities.js" library="javascript" target="head" />
</h:head>
<h:body styleClass="screenBg">
	<h:form id="formId" dir="#{msgs.dir}">
		<ui:decorate template="/Main/Template.xhtml">

			<table border="0" cellpadding="0" cellspacing="0" width="100%">
				<tr>
					<td width="100%" height="5" />
				</tr>
				<tr>
					<td class="screenTitle">#{msgs.title_laboratoryTestReport}</td>
				</tr>

				<tr>
					<td width="100%" height="5" />
				</tr>

				<tr>
					<td colspan="2">
						<table class="sectionTitle" border="0" cellpadding="0" cellspacing="0">
							<tr>
								<td>
									<h:outputText value="#{msgs.label_searchData}" />
								</td>
							</tr>
						</table>
					</td>
				</tr>

				<tr>
					<td width="100%" height="5" />
				</tr>

				<tr>
					<td>
						<h:panelGroup id="searchContainerId">
							<table class="sectionContent" border="0" cellpadding="0" cellspacing="0">

								<tr>
									<th width="15%">#{msgs.label_region}</th>
									<td width="20%">
										<h:selectOneMenu rendered="#{empty laboratoryTestReport.employeeRegionName}" value="#{laboratoryTestReport.regionId}">
											<f:selectItem itemLabel="#{msgs.label_all}" itemValue="-1" />
											<f:selectItems value="#{laboratoryTestReport.regionDepartmentsList}" var="dep" itemLabel="#{dep.arabicName}" itemValue="#{dep.id}" />
										</h:selectOneMenu>
										<h:outputText rendered="#{not empty laboratoryTestReport.employeeRegionName}" value="#{laboratoryTestReport.employeeRegionName}" />
									</td>


									<th width="15%">#{msgs.label_checkResult}</th>
									<td width="20%">
										<h:selectOneMenu value="#{laboratoryTestReport.labCheckResult}" styleClass="form-control">
											<f:selectItem itemLabel="" itemValue="" />
											<f:selectItem itemLabel="#{msgs.label_noSample}" itemValue="#{laboratoryTestReport.noSampleCheckResult}" />
											<f:selectItem itemLabel="#{msgs.label_sampleTaked}" itemValue="#{laboratoryTestReport.sampleTakedResult}" />
											<f:selectItem itemLabel="#{msgs.label_checkPrivation}" itemValue="#{laboratoryTestReport.checkPrivationCheckResult}" />
											<f:selectItem itemLabel="#{msgs.label_negative}" itemValue="#{laboratoryTestReport.negativeCheckResult}" />
											<f:selectItem itemLabel="#{msgs.label_positiveUnderApprove}" itemValue="#{laboratoryTestReport.positiveUnderApproveCheckResult}" />
											<f:selectItem itemLabel="#{msgs.label_positive}" itemValue="#{laboratoryTestReport.positiveCheckResult}" />
											<f:selectItem itemLabel="#{msgs.label_sendingToForce}" itemValue="#{laboratoryTestReport.sendingToForceCheckResult}" />
											<f:selectItem itemLabel="#{msgs.label_retest}" itemValue="#{laboratoryTestReport.retestCheckResult}" />
											<f:selectItem itemLabel="#{msgs.label_cheatingInSample}" itemValue="#{laboratoryTestReport.cheatingInCheckResult}" />
										</h:selectOneMenu>
									</td>

								</tr>

								<tr>
									<th width="15%">#{msgs.label_checkReason}</th>
									<td width="20%">
										<h:selectOneMenu value="#{laboratoryTestReport.labCheckReason}" styleClass="form-control">
											<f:selectItem itemLabel="" itemValue="" />
											<f:selectItems value="#{laboratoryTestReport.labCheckReasonsEnumList}" var="checkReason" itemLabel="#{checkReason.arabicDescription}" itemValue="#{checkReason.code}" />
										</h:selectOneMenu>
									</td>

								</tr>

								<tr>
									<th width="15%">
										<h:outputText value="*" style="color:red;" />
										#{msgs.label_fromDate}
									</th>
									<td width="20%">
										<util:advancedHijriDate value="#{laboratoryTestReport.fromDate}" />
									</td>

									<th width="15%">
										<h:outputText value="*" style="color:red;" />
										#{msgs.label_toDate}
									</th>
									<td width="20%">
										<util:advancedHijriDate value="#{laboratoryTestReport.toDate}" />
									</td>

								</tr>

							</table>
						</h:panelGroup>
					</td>
				</tr>

				<tr>
					<td width="100%" height="5" />
				</tr>

				<tr>
					<td width="100%">
						<table class="actionsSection" border="0" cellpadding="0" cellspacing="0">
							<tr>
								<td width="100%">
									<h:commandButton class="btn btn-success" value="&#xf021; #{msgs.label_reset}" title="#{msgs.label_reset}" action="#{laboratoryTestReport.reset()}">
										<f:ajax render="formId:searchContainerId" />
									</h:commandButton>
									<h:commandButton class="btn btn-success" value="&#xf0f6; #{msgs.label_printStatisticalReport}" title="#{msgs.label_printStatisticalReport}" action="#{laboratoryTestReport.printReportStatistics()}" />
									<h:commandButton class="btn btn-success" value="&#xf02f; #{msgs.label_print}" title="#{msgs.label_print}" action="#{laboratoryTestReport.printReport()}" />
								</td>
							</tr>
						</table>
					</td>
				</tr>

			</table>
		</ui:decorate>
	</h:form>
</h:body>
</html>