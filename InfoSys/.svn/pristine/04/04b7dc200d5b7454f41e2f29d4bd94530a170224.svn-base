	<parameter name="P_SCHEMA_NAME" class="java.lang.String">
		<defaultValueExpression><![CDATA["INFOSYS"]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select $P!{P_SCHEMA_NAME}.PKG_CONCAT($P!{P_SCHEMA_NAME}.PKG_DATE_TO_CHAR (o.ORDER_DATE, 'YYYY/MM/MI') ,'هـ') ORDER_DATE,
o.ORDER_NUMBER, $P!{P_SCHEMA_NAME}.PKG_DATE_TO_CHAR (r.END_DATE, 'MI/MM/YYYY') END_DATE,
$P!{P_SCHEMA_NAME}.PKG_CONCAT($P!{P_SCHEMA_NAME}.PKG_DATE_TO_CHAR (r.END_DATE, 'YYYY/MM/MI') ,'هـ') END_DATE_FORMATTED,
$P!{P_SCHEMA_NAME}.PKG_CONCAT(o.PERIODIC_REPORTING , ' شهر') PERIODIC_REPORTING,
$P!{P_SCHEMA_NAME}.PKG_DATE_TO_CHAR (r.STRAT_DATE, 'MI/MM/YYYY') START_DATE,
$P!{P_SCHEMA_NAME}.PKG_CONCAT($P!{P_SCHEMA_NAME}.PKG_DATE_TO_CHAR (r.STRAT_DATE, 'YYYY/MM/MI') ,'هـ') START_DATE_FORMATTED,
se.SURVEILLANCE_REASONS , o.FINAL_APPROVAL_ENTITY,
$P!{P_SCHEMA_NAME}.PKG_CONCAT($P!{P_SCHEMA_NAME}.PKG_DATE_TO_CHAR (o.START_DATE, 'YYYY/MM/MI'),'هـ') ORDER_START_DATE,
se.ID SUR_EMP_ID,
se.FULL_NAME, se.MILITARY_NO, se.RANK, se.SOCIAL_ID, ed.HOME_DESCRIPTION, ed.MARITAL_STATUS, ed.STP_QUALIFICATION_DESCRIPTION,
ed.PHONE_NUMBER, se.STP_VW_EMPLOYEES_ID EMP_ID, se.ACTUAL_DEPARTMENT_NAME,
CASE se.BIRTH_DATE
WHEN null THEN '' ELSE $P!{P_SCHEMA_NAME}.PKG_CONCAT($P!{P_SCHEMA_NAME}.PKG_DATE_TO_CHAR (se.BIRTH_DATE, 'YYYY/MM/MI'),'هـ') END BIRTH_DATE
from $P!{P_SCHEMA_NAME}.FIS_SURVEILLANCE_REPORTS r, $P!{P_SCHEMA_NAME}.FIS_VW_SURVEILLANCE_ORDERS o, $P!{P_SCHEMA_NAME}.FIS_VW_SURVEILLANC_EMP_NON_EMP se LEFT OUTER JOIN $P!{P_SCHEMA_NAME}.FIS_EMP_NON_EMP_DETAILS ed ON se.STP_VW_EMPLOYEES_ID = ed.EMP_ID
where r.SURVEILLANCE_EMPLOYEES_ID = se.ID
and se.FIS_SURVEILLANCE_ORDERS_ID = o.ID
and r.ID = $P{P_SURVEILLANCE_REPORT_ID}]]>
	</queryString>
