 CASE lc.CASE_DATE WHEN null THEN '' ELSE $P!{P_SCHEMA_NAME}.PKG_CONCAT($P!{P_SCHEMA_NAME}.PKG_DATE_TO_CHAR(lc.CASE_DATE, 'YYYY/MM/MI'),'هـ') END CASE_DATE, lc.CASE_NUMBER, lc.CHECK_REASON,
CASE lc.CHECK_REASON
WHEN 1 THEN 'الترقية'
WHEN 2 THEN 'مفاجئ'
WHEN 3 THEN 'قضايا'
WHEN 4 THEN 'مهام مديرية'
WHEN 5 THEN 'عرض على اللجنة الطبية'
WHEN 7 THEN 'دورة'
WHEN 6 THEN 'آلية علاج' END CHECK_REASON_DESC
from $P!{P_SCHEMA_NAME}.FIS_LAB_CHECKS lc
where lc.ID = $P{P_LAB_CHECK_ID}]]>
