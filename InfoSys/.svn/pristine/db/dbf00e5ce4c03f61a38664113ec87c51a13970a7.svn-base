	<queryString>
		<![CDATA[select mrdd.AGENT_CODE, mrdd.AGENT_NAME, mrdd.AGENT_REGION_NAME, mrdd.AGENT_SECTOR_NAME,
CASE mrdd.AGENT_PAYMENT_METHOD
WHEN 0 THEN 'حساب بنكي'
WHEN 1 THEN 'شيك'
WHEN 2 THEN 'نقدى'END AGENT_PAYMENT_METHOD, mrdd.AGENT_MONTHLY_REWARD,
 CASE mrdd.AGENT_IBAN WHEN null THEN '' ELSE $P!{P_SCHEMA_NAME}.PKG_CONCAT('SA' ,mrdd.AGENT_IBAN) END AGENT_IBAN
from $P!{P_SCHEMA_NAME}.FIS_VW_MONTHLY_REWRD_DEPT_DTLS mrdd
where mrdd.MONTHLY_REWARDS_ID = $P{P_MONTHLY_REWARD_ID}
order by mrdd.AGENT_SECTOR_NAME]]>
	</queryString>
