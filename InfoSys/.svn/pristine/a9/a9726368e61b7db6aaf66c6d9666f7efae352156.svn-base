	<queryString>
		<![CDATA[SELECT DETAILS.START_DATE,DETAILS.END_DATE,DETAILS.APPROVED_END_DATE,EVAL_REASON.REASONS
FROM $P!{P_SCHEMA_NAME}.FIS_VW_ASSIGNMENTS_DETAILS DETAILS LEFT OUTER JOIN (select max(ID) as id, ASSIGNMENT_DETAILS_ID from $P!{P_SCHEMA_NAME}.FIS_ASSIGNMENT_EVALUATIONS GROUP BY ASSIGNMENT_DETAILS_ID )  EVAL ON DETAILS.ID = EVAL.ASSIGNMENT_DETAILS_ID
   LEFT OUTER JOIN $P!{P_SCHEMA_NAME}.FIS_ASSIGNMENT_EVALUATIONS EVAL_REASON ON EVAL.id = EVAL_REASON.ID
 INNER JOIN $P!{P_SCHEMA_NAME}.FIS_WF_INSTANCES INST ON DETAILS.WF_INSTANCES_ID = INST.ID
WHERE  INST.STATUS              <> 1
AND DETAILS."IDENTITY" = $P{P_IDENTITY}
AND DETAILS.START_DATE < $P{P_START_DATE}]]>
	</queryString>
