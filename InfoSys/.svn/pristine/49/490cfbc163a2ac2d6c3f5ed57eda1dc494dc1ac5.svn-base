<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN"
  "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml" xmlns:h="http://java.sun.com/jsf/html" xmlns:p="http://primefaces.org/ui" xmlns:ui="http://java.sun.com/jsf/facelets" xmlns:f="http://java.sun.com/jsf/core" xmlns:util="http://java.sun.com/jsf/composite/util">
<h:head>
	<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
	<h:outputStylesheet library="css" name="styles.css" />
	<h:outputStylesheet library="css" name="grid.css" />
	<h:outputStylesheet library="css" name="bootstrap.css" />
	
	<h:outputScript name="utilities.js" library="javascript" target="head" />
	<script type="text/javascript">
	var m;
	function getEmployee(contextPath,mode){
		m=mode;
		openEmpsMiniSearch(contextPath);
   		return false;
   	}

	function releaseMask(){
		unMaskScreen();
   	}
   	
	function HandlePopupResult(ret) { 
		unMaskScreen();
		if(ret!=null){ 
			if(m==1){
				document.getElementById('carRentalReport:selectedEmpId').value = ret[0];
   	         	document.getElementById('carRentalReport:selectedEmpNameId').value = ret[1];
   	         	document.getElementById('carRentalReport:empNameId').value= ret[1];
   	         	return true;
			}
   		}
   		return false;
	}

     </script>
</h:head>
<h:body styleClass="screenBg">
	<h:form id="carRentalReport" dir="#{msgs.dir}">
			<ui:decorate template="/Main/Template.xhtml">
			<table border="0" cellpadding="0" cellspacing="0" width="100%">
				<tr>
					<td class="screenTitle">#{msgs.title_carRntReport}</td>
				</tr>

				<tr>
					<td width="100%" height="5" />
				</tr>
				
				<tr>
					<td colspan="2">
						<table class="sectionTitle" border="0" cellpadding="0" cellspacing="0">
							<tr>
								<td>
									<h:outputText value="#{msgs.label_reportData}" />
								</td>
							</tr>
						</table>
					</td>
				</tr>
				
				<tr>
					<td width="100%" height="5" />
				</tr>
				
				<tr>
					<td>
						<h:panelGroup id="carRentalReportContainer">
							<table class="sectionContent" border="0" cellpadding="0" cellspacing="0">
								<tr>
									<th width="15%">
										#{msgs.label_renterName}
										<h:commandButton value="&#xf002;" styleClass="btn btn-trans-icon" onclick="return getEmployee('#{request.contextPath}',1);" title="#{msgs.label_renterName}" />
									</th>
									<td width="20%">
										<h:inputHidden id="selectedEmpId" value="#{carRentalReport.renterId}" />
										<h:inputHidden id="selectedEmpNameId" value="#{carRentalReport.renterName}" />
										<h:inputText styleClass="form-control" id="empNameId" disabled="true" value="#{carRentalReport.renterName}" />
									</td>
									 
									
									<th width="15%">#{msgs.label_region}</th>
									<td width="20%">
										<h:selectOneMenu id="regionMenuId" value="#{carRentalReport.regionId}">
											<f:selectItem itemValue="-1" itemLabel="#{msgs.label_all}" />
											<f:selectItems value="#{carRentalReport.regionList}" var="var" itemLabel="#{var.arabicName}" itemValue="#{var.id}"/>
										</h:selectOneMenu>
									</td>
									
								</tr>
								
								<tr>
									<th width="15%">#{msgs.label_contractStatus}</th>
									<td width="20%" >
										<h:selectOneMenu styleClass="form-control" value="#{carRentalReport.contractStatus}">
											<f:selectItem itemValue="-1" itemLabel="#{msgs.label_all}" />
											<f:selectItem itemValue="1" itemLabel="#{msgs.label_paidRental}" />
											<f:selectItem itemValue="0" itemLabel="#{msgs.label_unpaidRental}" />
										</h:selectOneMenu>
									</td>
									 <td width="65%" colspan="6"></td>
								</tr>
								
								<tr>
									<th width="15%">#{msgs.label_rentalStartDate} #{msgs.label_from}<h:outputText value="*" style="color:red;" /></th>
									<td width="20%">
										<util:advancedHijriDate value="#{carRentalReport.rentalStartDate}" />
									</td>
									 
									<th width="15%">#{msgs.label_rentalEndDate} #{msgs.label_to}<h:outputText value="*" style="color:red;" /></th>
									<td width="20%">
										<util:advancedHijriDate value="#{carRentalReport.rentalEndDate}" />
									</td>
									
								</tr>
							</table>
						</h:panelGroup>
					</td>
				</tr>
				<tr>
					<td width="100%" height="5" />
				</tr>
				<tr>
					<td width="100%">
						<table class="actionsSection" border="0" cellpadding="0" cellspacing="0">
							<tr>
								<td>
									<h:commandButton styleClass="btn btn-default" id="resetBtn" value="&#xf021; #{msgs.label_reset}" action="#{carRentalReport.resetFormParameters}">
										<f:ajax render="carRentalReport:carRentalReportContainer" />
									</h:commandButton>
									<h:commandButton styleClass="btn btn-default" id="printStatisticsBtn" value="&#xf0f6; #{msgs.label_printStatisticalReport}" action="#{carRentalReport.printStatistics()}">
									</h:commandButton>
									<h:commandButton styleClass="btn btn-default" id="printBtn" value="&#xf02f; #{msgs.label_print}" action="#{carRentalReport.print()}">
									</h:commandButton>
								</td>
							</tr>
						</table>
					</td>
				</tr>
			</table>
		</ui:decorate>
	</h:form>
</h:body>
</html>