<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN"
  "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml" xmlns:code="http://java.sun.com/jsf/composite/code" xmlns:of="http://omnifaces.org/functions" xmlns:o="http://omnifaces.org/ui" xmlns:h="http://java.sun.com/jsf/html" xmlns:util="http://java.sun.com/jsf/composite/util" xmlns:p="http://primefaces.org/ui" xmlns:ui="http://java.sun.com/jsf/facelets" xmlns:f="http://java.sun.com/jsf/core">
<h:head>
	<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
	<h:outputStylesheet library="css" name="styles.css" />
	<h:outputStylesheet library="css" name="grid.css" />
	<h:outputStylesheet library="css" name="bootstrap.css" />
	<h:outputScript name="utilities.js" library="javascript" target="head" />

	<script type="text/javascript">
		window.onunload = unloadPage;
	
	    function unloadPage(){
	  	  window.opener.releaseMask();
	    }
     </script>

</h:head>
<h:body styleClass="screenBg">
	<h:form id="assignmentFormId" dir="#{msgs.dir}">
		<table border="0" cellpadding="0" cellspacing="0" width="100%" class="bgcolor-pup">
			<tr>
				<td width="100%" height="5" />
			</tr>

			<tr>
				<td class="screenTitle">
					<h:outputText value="#{nonEmployeeAssginmentView.screenTitle}" />
				</td>
			</tr>

			<tr>
				<td width="100%" height="5" />
			</tr>

			<tr>
				<td colspan="2">
					<table class="sectionTitle" border="0" cellpadding="0" cellspacing="0">
						<tr>
							<td>
								<h:outputText value="#{msgs.label_assginmentOrder}" />
							</td>
						</tr>
					</table>
				</td>
			</tr>

			<tr>
				<td width="100%" height="5" />
			</tr>

			<tr>
				<td>
					<h:panelGroup id="assignmentContainerId">
						<table class="sectionContent" border="0" cellpadding="0" cellspacing="0">
							<tr>
								<th>#{msgs.label_assignmentRequestNumber}</th>
								<td>
									<h:outputText value="#{nonEmployeeAssginmentView.assignmentData.requestNumber}" />
								</td>
								

								<th>#{msgs.label_assignmentRequestData}</th>
								<td>
									<util:advancedHijriDate disable="true" value="#{nonEmployeeAssginmentView.assignmentData.requestDate}" />
								</td>
								
							</tr>

							<tr>
								<th width="15%">#{msgs.label_regionHq}</th>
								<td width="25%">
									<h:outputText id="regionNameId" value="#{nonEmployeeAssginmentView.assignmentData.regionName}" />
								</td>
								
								<th width="15%">
									<h:panelGroup id="sectorPaneId">
										<h:outputText rendered="#{nonEmployeeAssginmentView.assignmentData.regionId != nonEmployeeAssginmentView.headQuarterId}" value="#{msgs.label_sector}" />
									</h:panelGroup>
								</th>
								<td width="25%">
									<h:outputText id="departmentNameId" value="#{nonEmployeeAssginmentView.assignmentData.sectorName}" />
								</td>
								
							</tr>

							<tr>
								<th>#{msgs.label_assignmentOfficer}</th>
								<td width="25%">
									<h:outputText value="#{nonEmployeeAssginmentView.assignmentData.officerName}" />
								</td>
								<td colspan="4"></td>

							</tr>

							<tr>
								<th>#{msgs.label_workScope}</th>
								<td width="25%">
									<h:inputTextarea maxlength="255" rows="3" cols="50" styleClass="form-control" disabled="true" value="#{nonEmployeeAssginmentView.assignmentDetailData.workScope}" />
								</td>
								<td colspan="4"></td>
							</tr>

							<tr>
								<th>#{msgs.label_assignmentNonEmpStartDate}</th>
								<td>
									<util:advancedHijriDate disable="true" value="#{nonEmployeeAssginmentView.assignmentData.startDate}" />
								</td>
								

								<th>#{msgs.label_assignmentNonEmpPeriod}</th>
								<td>
									<h:selectOneMenu disabled="true" styleClass="selectMenuWithIcon" value="#{nonEmployeeAssginmentView.assignmentData.period}">
										<f:selectItem itemValue="1" itemLabel="1" />
										<f:selectItem itemValue="2" itemLabel="2" />
										<f:selectItem itemValue="3" itemLabel="3" />
										<f:selectItem itemValue="4" itemLabel="4" />
										<f:selectItem itemValue="5" itemLabel="5" />
										<f:selectItem itemValue="6" itemLabel="6" />
										<f:selectItem itemValue="7" itemLabel="7" />
										<f:selectItem itemValue="8" itemLabel="8" />
										<f:selectItem itemValue="9" itemLabel="9" />
										<f:selectItem itemValue="10" itemLabel="10" />
										<f:selectItem itemValue="11" itemLabel="11" />
										<f:selectItem itemValue="12" itemLabel="12" />
									</h:selectOneMenu>
									#{msgs.label_month}
								</td>
								
							</tr>

							<tr>
								<td>
									<p:selectCheckboxMenu label="#{msgs.label_assignmentReasons}" styleClass="btn btn_normal" value="#{nonEmployeeAssginmentView.assignmentDetailData.reasonsList}" filter="true" filterMatchMode="startsWith">
										<f:selectItems itemDisabled="true" value="#{nonEmployeeAssginmentView.assignmentReasonsList}" var="reason" itemLabel="#{reason}" itemValue="#{reason}" />
									</p:selectCheckboxMenu>
								</td>
							</tr>
						</table>
					</h:panelGroup>
				</td>
			</tr>

			<tr>
				<td width="100%" height="5" />
			</tr>

			<tr>
				<td>
					<h:panelGroup id="assignmentDetailsPaneId">
						<table class="sectionContent" border="0" cellpadding="0" cellspacing="0">
							<tr>
								<td colspan="8">
									<table class="sectionTitle" border="0" cellpadding="0" cellspacing="0">
										<tr>
											<td>
												<h:outputText value="#{msgs.label_agentDetails}" />
											</td>
										</tr>
									</table>
								</td>
							</tr>

							<tr>
								<th>#{msgs.label_identity}</th>
								<td>
									<h:inputText maxlength="10" styleClass="form-control" disabled="true" onblur="return isDigitFloat(this)" onkeypress="return checkDigit(event);" value="#{nonEmployeeAssginmentView.assignmentDetailData.identity}" />
								</td>
								

								<th>#{msgs.label_agentCode}</th>
								<td>
									<h:outputText value="#{nonEmployeeAssginmentView.assignmentDetailData.agentCode} " />
								</td>
								
							</tr>

							<tr>
								<th>#{msgs.label_name}</th>
								<td>
									<h:inputText maxlength="255" styleClass="form-control" disabled="true" value="#{nonEmployeeAssginmentView.assignmentDetailData.fullName}" />
								</td>
								

								<th>#{msgs.label_birthDate}</th>
								<td>
									<util:advancedHijriDate disable="true" value="#{nonEmployeeAssginmentView.assignmentDetailData.birthDate}" />
								</td>
								
							</tr>

							<tr>
								<th width="15%">#{msgs.label_nationality}</th>
								<td width="20%">
									<h:outputText id="countryNameId" value="#{nonEmployeeAssginmentView.assignmentDetailData.countryName}" />
								</td>
								
								<th width="15%">#{msgs.label_workPlace}</th>

								<td width="20%">
									<h:inputText maxlength="200" styleClass="form-control" disabled="true" value="#{nonEmployeeAssginmentView.assignmentDetailData.workPlace}" />
								</td>
								
							</tr>

							<tr>
								<th width="15%">#{msgs.label_phoneNumbers}</th>
								<td width="20%">
									<h:inputText maxlength="15" onblur="return isPhoneNumber(this)" onkeypress="return checkPhoneNumber(event);" styleClass="form-control" disabled="true" value="#{nonEmployeeAssginmentView.assignmentDetailData.primaryPhone}" />
									<h:inputText maxlength="15" onblur="return isPhoneNumber(this)" onkeypress="return checkPhoneNumber(event);" styleClass="form-control" disabled="true" value="#{nonEmployeeAssginmentView.assignmentDetailData.secondaryPhone}" />
									<h:inputText maxlength="15" onblur="return isPhoneNumber(this)" onkeypress="return checkPhoneNumber(event);" styleClass="form-control" disabled="true" value="#{nonEmployeeAssginmentView.assignmentDetailData.ternaryPhone}" />
								</td>
							</tr>

							<tr>
								<th width="15%">#{msgs.label_assignmentType}</th>
								<td width="20%">
									<h:selectOneMenu disabled="true" value="#{nonEmployeeAssginmentView.assignmentDetailData.type}" styleClass="form-control">
										<f:selectItem itemLabel="#{msgs.label_assignment}" itemValue="#{nonEmployeeAssginmentView.assignment}" />
										<f:selectItem itemLabel="#{msgs.label_cooperator}" itemValue="#{nonEmployeeAssginmentView.cooperetor}" />
									</h:selectOneMenu>
								</td>
								
								<td colspan="5">
									<h:selectOneRadio disabled="true" value="#{nonEmployeeAssginmentView.assignmentDetailData.agentType}">
										<f:selectItem itemValue="0" itemLabel="#{msgs.label_military}" />
										<f:selectItem itemValue="1" itemLabel="#{msgs.label_civil}" />
									</h:selectOneRadio>
								</td>
							</tr>
							<tr>
								<td colspan="5">
									<h:selectOneRadio disabled="true" value="#{nonEmployeeAssginmentView.assignmentDetailData.location}">
										<f:selectItem itemValue="0" itemLabel="#{msgs.label_outside}" />
										<f:selectItem itemValue="1" itemLabel="#{msgs.label_inside}" />
									</h:selectOneRadio>
								</td>
							</tr>

							<tr>
								<td colspan="5">
									<h:panelGroup id="paymentPanelId" rendered="#{nonEmployeeAssginmentView.assignmentDetailData.type == nonEmployeeAssginmentView.assignment}">
										<table style=" width: 100%; ">
												<tr>
												<th width="15%">#{msgs.label_agentClass}</th>
												<td width="20%">
													<h:selectOneMenu styleClass="form-control" disabled="true" value="#{nonEmployeeAssginmentView.assignmentDetailData.agentClass}">
														<f:selectItems value="#{nonEmployeeAssginmentView.assignmentAgentClassesList}" var="class" itemValue="#{class.code}" itemLabel="#{class.code}" />
													</h:selectOneMenu>
												</td>
												<td width="15%" />
												<th width="15%">#{msgs.label_monthlyReward}</th>
												<td width="20%">
													<h:outputText id="monthlyRewardId" value="#{nonEmployeeAssginmentView.assignmentDetailData.monthlyReward}" />
													#{msgs.label_riyal}
												</td>
											</tr>

											<tr>
												<th width="15%">#{msgs.label_paymentMethod}</th>
												<td width="20%">
													<h:selectOneMenu styleClass="form-control" disabled="true" value="#{nonEmployeeAssginmentView.assignmentDetailData.paymentMethod}">
														<f:selectItem itemValue="#{nonEmployeeAssginmentView.bankAccountEnum}" itemLabel="#{msgs.label_bankAccount}" />
														<f:selectItem itemValue="#{nonEmployeeAssginmentView.chequeAcountEnum}" itemLabel="#{msgs.label_cheque}" />
														<f:selectItem itemValue="#{nonEmployeeAssginmentView.cashAccountEnum}" itemLabel="#{msgs.label_cash}" />
													</h:selectOneMenu>
												</td>
												<td width="15%" />

												<td colspan="2">
													<h:panelGroup id="paymentMethodId">
														<h:panelGroup rendered="#{nonEmployeeAssginmentView.assignmentDetailData.paymentMethod == nonEmployeeAssginmentView.bankAccountEnum}">
															<table>
																<tr>
																	<th width="15%">
																		<h:outputText value="#{msgs.label_bankAccount}" />
																	</th>
																	<td width="20%">
																		<h:selectOneMenu disabled="true" styleClass="form-control" value="#{nonEmployeeAssginmentView.assignmentDetailData.bankBranchId}">
																			<f:selectItems value="#{nonEmployeeAssginmentView.bankBranchesDataList}" var="bank" itemValue="#{bank.id}" itemLabel="#{bank.name}" />
																		</h:selectOneMenu>
																		<h:inputText style="width:190px" maxlength="22" styleClass="form-control" disabled="true" onblur="return isDigitFloat(this)" onkeypress="return checkDigit(event);" value="#{nonEmployeeAssginmentView.assignmentDetailData.iban}" />
																		SA
																	</td>
																</tr>
															</table>
														</h:panelGroup>
													</h:panelGroup>
												</td>
											</tr>
										</table>
									</h:panelGroup>
								</td>
							</tr>
						</table>
					</h:panelGroup>
				</td>
			</tr>
		</table>
	</h:form>
</h:body>
</html>