	<queryString>
		<![CDATA[SELECT D.ARABIC_NAME DEPARTMENT_NAME, R.ARABIC_NAME REGION_NAME,
COUNT(DISTINCT IR.ASSIGNMENT_DETAILS_ID) ASSIGNMENT_COUNT, SUM(IR.AMOUNT) AMOUNT
FROM $P!{P_SCHEMA_NAME}.FIS_INFORMATION_REWARDS IR,
$P!{P_SCHEMA_NAME}.FIS_ASSIGNMENTS_DETAILS AD,
  $P!{P_SCHEMA_NAME}.STP_VW_DEPARTMENTS D,
  $P!{P_SCHEMA_NAME}.STP_VW_DEPARTMENTS R,
  $P!{P_SCHEMA_NAME}.STP_VW_DEPARTMENTS_SECOTRS SECT
WHERE IR.STP_VW_DEPARTMENT_ID = D.ID
AND D.REGIONID  = R.ID
AND D.ID = SECT.ID
AND IR.ASSIGNMENT_DETAILS_ID = AD.ID
AND ($P{P_REGION_ID} = -1 OR D.REGIONID = $P{P_REGION_ID} OR D.ID = $P{P_REGION_ID})
AND ($P{P_SECTOR_ID} = -1 OR SECT.SECTORID = $P{P_SECTOR_ID} OR D.ID = $P{P_SECTOR_ID})
AND ($P{P_START_DATE_NULL} = 1 OR IR.PAYMENT_DATE >= $P!{P_SCHEMA_NAME}.PKG_CHAR_TO_DATE ($P{P_START_DATE},'MI/mm/YYYY'))
AND ($P{P_END_DATE_NULL} = 1 OR IR.PAYMENT_DATE <= $P!{P_SCHEMA_NAME}.PKG_CHAR_TO_DATE ($P{P_END_DATE},'MI/mm/YYYY'))
AND ( $P{P_ASSIGNMENT_TYPE} = -1 OR   AD.TYPE = $P{P_ASSIGNMENT_TYPE}  )
GROUP BY D.ARABIC_NAME, R.ARABIC_NAME
ORDER BY REGION_NAME, DEPARTMENT_NAME]]>
	</queryString>
