	</parameter>
	<queryString>
		<![CDATA[SELECT DSD.DEPARTMENT_NAME,
$P!{P_SCHEMA_NAME}.PKG_SUBSTRING(DSD.DEPARTMENT_NAME, INFOSYS.PKG_INSTR(DSD.DEPARTMENT_NAME, '/')+1, INFOSYS.PKG_LENGTH(DSD.DEPARTMENT_NAME)) as DEP_NAME,
  R.ARABIC_NAME REGION_NAME,
  SUM(DSD.SUPPORT_AMOUNT) SUPPORT_AMOUNT,
  (SELECT (CASE WHEN SUM(IR.AMOUNT) IS NULL THEN 0 ELSE SUM(IR.AMOUNT) END) TOTAL_AMOUNT
  FROM INFOSYS.FIS_INFORMATION_REWARDS IR
  WHERE IR.STP_VW_DEPARTMENT_ID = DSD.STP_VW_DEPARTMENTS_ID
  AND IR.FIN_YEAR_APPROVAL_ID   = DS.FIS_FIN_YEAR_APPROVALS_ID
  ) SPENT_AMOUNT
FROM INFOSYS.FIS_DEPARTMENTS_SUPPORTS DS,
  INFOSYS.FIS_VW_DEPS_SUPPORT_DETAILS DSD,
  INFOSYS.STP_VW_DEPARTMENTS D,
  INFOSYS.STP_VW_DEPARTMENTS R,
  INFOSYS.STP_VW_DEPARTMENTS_SECOTRS S
WHERE DS.ID     = DSD.DEPARTMENTS_SUPPORTS_ID
AND D.ID        = DSD.STP_VW_DEPARTMENTS_ID
AND D.REGIONID  = R.ID
AND D.ID = S.ID
AND DS.APPROVED = 1
AND ($P{P_START_DATE_NULL} = 1 OR DS.REQUEST_DATE >= INFOSYS.PKG_CHAR_TO_DATE ($P{P_START_DATE},'MI/mm/YYYY'))
AND ($P{P_END_DATE_NULL} = 1 OR DS.REQUEST_DATE <= INFOSYS.PKG_CHAR_TO_DATE ($P{P_END_DATE},'MI/mm/YYYY'))
AND ($P{P_REGION_ID} = -1 OR D.REGIONID = $P{P_REGION_ID} OR D.ID = $P{P_REGION_ID})
AND ($P{P_SECTOR_ID} = -1 OR S.SECTORID = $P{P_SECTOR_ID} OR D.ID = $P{P_SECTOR_ID})
GROUP BY DSD.DEPARTMENT_NAME,
  R.ARABIC_NAME,
  DSD.STP_VW_DEPARTMENTS_ID,
  DS.FIS_FIN_YEAR_APPROVALS_ID
ORDER BY REGION_NAME, DEPARTMENT_NAME]]>
	</queryString>
