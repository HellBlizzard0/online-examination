	<queryString>
		<![CDATA[select $P!{P_SCHEMA_NAME}.PKG_CONCAT($P!{P_SCHEMA_NAME}.PKG_DATE_TO_CHAR (mr.REQUEST_DATE, 'YYYY/MM/MI'),'هـ') REQUEST_DATE ,mr.REQUEST_NUMBER, CASE mr.MONTH_NUMBER
WHEN 1 THEN 'محرم'
WHEN 2 THEN 'صفر'
WHEN 3 THEN 'ربيع أول'
WHEN 4 THEN 'ربيع آخر'
WHEN 5 THEN 'جماد أول'
WHEN 6 THEN 'جماد آخر'
WHEN 7 THEN 'رجب'
WHEN 8 THEN 'شعبان'
WHEN 9 THEN 'رمضان'
WHEN 10 THEN 'شوال'
WHEN 11 THEN 'ذو القعدة'
WHEN 12 THEN 'ذو الحجة' END MONTH_NAME, $P!{P_SCHEMA_NAME}.PKG_CONCAT(mr.ACCOUNT_BALANCE , ' ريال') ACCOUNT_BALANCE,$P!{P_SCHEMA_NAME}.PKG_CONCAT( mr.TOTAL_AMOUNT , ' ريال') TOTAL_AMOUNT,$P!{P_SCHEMA_NAME}.PKG_CONCAT( mr.HIJRI_YEAR ,'هـ') HIJRI_YEAR
from $P!{P_SCHEMA_NAME}.FIS_MONTHLY_REWARDS mr
where mr.ID = $P{P_MONTHLY_REWARD_ID}]]>
	</queryString>
