<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN"
  "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml" xmlns:h="http://java.sun.com/jsf/html" xmlns:p="http://primefaces.org/ui" xmlns:ui="http://java.sun.com/jsf/facelets" xmlns:f="http://java.sun.com/jsf/core" xmlns:util="http://java.sun.com/jsf/composite/util">
<h:head>
	<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
	<h:outputStylesheet library="css" name="styles.css" />
	<h:outputStylesheet library="css" name="grid.css" />
	<h:outputStylesheet library="css" name="bootstrap.css" />

	<h:outputScript name="utilities.js" library="javascript" target="head" />
	<script type="text/javascript">
		var m;
		function getEmployee(contextPath, mode) {
			m = 1;
			openEmpsMiniSearch(contextPath, mode);
			return false;
		}

		function releaseMask() {
			unMaskScreen();
		}

		function HandlePopupResult(ret) {
			unMaskScreen();
			if (ret != null) {
				if (m == 1) {
					document.getElementById('securityGuardMissionSearchFormId:selectedEmpId').value = ret[0];
					document.getElementById('securityGuardMissionSearchFormId:selectedEmpNameId').value = ret[1];
					document.getElementById('securityGuardMissionSearchFormId:empNameId').innerHTML = ret[1];
					return true;
				}
			}
			return false;
		}

		window.onkeypress = function(e) {
			var keyCode = window.event.keyCode || e.keyCode || e.which;
			if (keyCode == '13') {
				document.getElementById('securityGuardMissionSearchFormId:searchBtn').click();
				return false;
			}
		};

		function validateData() {
			var orderNumber = document.getElementById("securityGuardMissionSearchFormId:orderNumberId").value;
			var empId = document.getElementById("securityGuardMissionSearchFormId:selectedEmpId").value;
			var missionNumber = document.getElementById("securityGuardMissionSearchFormId:missionNumberId").value;
			/* <![CDATA[ */
			if ((orderNumber == null || orderNumber == "") && (empId == null || empId == "" || empId == "-1") && (missionNumber == null || missionNumber == "")) {
				showErrorMessage('#{msgs.error_securityGuardSearchMandatory}');
				return false;
			}
			/* ]]> */
			return true;
		}
	</script>
</h:head>
<h:body styleClass="screenBg">
