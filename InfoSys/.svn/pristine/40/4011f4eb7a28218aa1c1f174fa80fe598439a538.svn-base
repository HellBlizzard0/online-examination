												<p:column styleClass='centerText' headerText="#{msgs.label_received}">
													<h:commandButton styleClass="btn btn-default" id="receivedBtnId" value="#{msgs.label_received}" action="#{securityGuardMissionSearch.receiveMissionAttachments(null,null,equipment)}" rendered="#{equipment.receiveDate == null}">
