<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN"
  "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml" xmlns:code="http://java.sun.com/jsf/composite/code" xmlns:of="http://omnifaces.org/functions" xmlns:o="http://omnifaces.org/ui" xmlns:h="http://java.sun.com/jsf/html" xmlns:util="http://java.sun.com/jsf/composite/util" xmlns:p="http://primefaces.org/ui" xmlns:ui="http://java.sun.com/jsf/facelets" xmlns:f="http://java.sun.com/jsf/core">
<h:head>
	<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
	<h:outputStylesheet library="css" name="styles.css" />
	<h:outputStylesheet library="css" name="grid.css" />
	<h:outputStylesheet library="css" name="bootstrap.css" />

	<h:outputScript name="utilities.js" library="javascript" target="head" />
	<script type="text/javascript">
		window.onunload = unloadPage;

		function unloadPage() {
			window.opener.releaseMask();
		}
	</script>

</h:head>
<h:body styleClass="screenBg">
	<h:form id="assignmentFormId" dir="#{msgs.dir}">

		<p:growl showDetail="true" autoUpdate="true" id="notifyMessagesId" />
		<p:ajaxStatus onstart="PF('reload').show();" onsuccess="PF('reload').hide();" />
		<p:dialog id="reload" widgetVar="reload" modal="true" showHeader="false" resizable="false" closable="false">
			<h:graphicImage value="/resources/images/wait.png" alt="ai" />
		</p:dialog>

		<table border="0" cellpadding="0" cellspacing="0" width="100%" class="bgcolor-pup">
			<tr>
				<td width="100%" height="5" />
			</tr>

			<tr>
				<td class="screenTitle">
					<h:outputText value="#{msgs.title_extraAgentDetails}" />
				</td>
			</tr>

			<tr>
				<td width="100%" height="5" />
			</tr>

			<tr>
				<td>
					<table class="sectionContent" border="0" cellpadding="0" cellspacing="0">
						<tr>
							<th>#{msgs.label_assignmentRequestNumber}</th>
							<td>
								<h:outputText value="#{employeeAssginmentView.assignmentData.requestNumber}" />

							</td>


							<th>#{msgs.label_assignmentRequestData}</th>
							<td>
								<util:advancedHijriDate disable="true" value="#{employeeAssginmentView.assignmentData.requestDate}" />
							</td>

						</tr>

						<tr>
							<th width="15%">#{msgs.label_regionHq}</th>
							<td width="25%">
								<h:outputText id="regionNameId" value="#{employeeAssginmentView.assignmentData.regionName}" />
							</td>

							<h:panelGroup rendered="#{employeeAssginmentView.assignmentData.regionId != employeeAssginmentView.headQuarterId}">
								<th width="15%">#{msgs.label_sector}</th>
								<td width="25%">
									<h:outputText id="departmentNameId" value="#{employeeAssginmentView.assignmentData.sectorName}" />
								</td>

							</h:panelGroup>
						</tr>

						<tr>
							<th>#{msgs.label_assignmentOfficer}</th>
							<td>
								<h:outputText value="#{employeeAssginmentView.assignmentData.officerName}" />
							</td>
						</tr>

						<tr>
							<th>#{msgs.label_assignmentStartDate}</th>
							<td>
								<util:advancedHijriDate disable="true" value="#{employeeAssginmentView.assignmentDetailsData.startDate}" />
							</td>
							
							<th>#{msgs.label_assignmentPeriod}</th>
							<td>
								<h:selectOneMenu disabled="true" styleClass="selectMenuWithIcon" value="#{employeeAssginmentView.assignmentDetailsData.period}">
									<f:selectItem itemValue="1" itemLabel="1" />
									<f:selectItem itemValue="2" itemLabel="2" />
									<f:selectItem itemValue="3" itemLabel="3" />
									<f:selectItem itemValue="4" itemLabel="4" />
									<f:selectItem itemValue="5" itemLabel="5" />
									<f:selectItem itemValue="6" itemLabel="6" />
									<f:selectItem itemValue="7" itemLabel="7" />
									<f:selectItem itemValue="8" itemLabel="8" />
									<f:selectItem itemValue="9" itemLabel="9" />
									<f:selectItem itemValue="10" itemLabel="10" />
									<f:selectItem itemValue="11" itemLabel="11" />
									<f:selectItem itemValue="12" itemLabel="12" />
								</h:selectOneMenu>
								#{msgs.label_month}
							</td>

						</tr>

						<tr>
							<th>#{msgs.label_name}</th>
							<td>
								<h:outputText value="#{employeeAssginmentView.assignmentDetailsData.fullName}" />
							</td>
						</tr>
						<tr>
							<th>#{msgs.label_agentCode}</th>
							<td>
								<h:outputText value="#{employeeAssginmentView.assignmentDetailsData.agentCode}" />
							</td>
						</tr>
						<tr>
							<th>#{msgs.label_assignmentReasons}</th>
							<td>
								<p:selectCheckboxMenu value="#{employeeAssginmentView.assignmentDetailsData.reasonsList}" label="#{msgs.label_reasons}" styleClass="btn btn_normal" filter="true" filterMatchMode="startsWith">
									<f:selectItems itemDisabled="true" value="#{employeeAssginmentView.assignmentReasonsList}" var="reason" itemLabel="#{reason}" itemValue="#{reason}" />
								</p:selectCheckboxMenu>
							</td>
						</tr>


						<tr>
							<th>#{msgs.label_phoneNumbers}</th>
							<td colspan="1">
								<h:inputText maxlength="15" disabled="true" styleClass="form-control" value="#{employeeAssginmentView.assignmentDetailsData.primaryPhone}" />
								<h:inputText maxlength="15" disabled="true" styleClass="form-control" value="#{employeeAssginmentView.assignmentDetailsData.secondaryPhone}" />
								<h:inputText maxlength="15" disabled="true" styleClass="form-control" value="#{employeeAssginmentView.assignmentDetailsData.ternaryPhone}" />
							</td>

						</tr>

						<tr>
							<th>#{msgs.label_workNature}</th>
							<td colspan="2">
								<h:inputTextarea maxlength="255" disabled="true" styleClass="form-control" rows="4" cols="50" value="#{employeeAssginmentView.assignmentDetailsData.workNature}" />
							</td>


							<th>#{msgs.label_workScope}</th>
							<td colspan="2">
								<h:inputTextarea maxlength="255" disabled="true" styleClass="form-control" rows="4" cols="50" value="#{employeeAssginmentView.assignmentDetailsData.workScope}" />
							</td>

						</tr>
					</table>
				</td>
			</tr>

			<tr>
				<td width="100%" height="5" />
			</tr>
		</table>
	</h:form>
</h:body>
</html>