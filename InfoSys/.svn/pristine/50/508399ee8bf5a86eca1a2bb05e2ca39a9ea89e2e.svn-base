<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN"
  "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml" xmlns:h="http://java.sun.com/jsf/html" xmlns:p="http://primefaces.org/ui" xmlns:ui="http://java.sun.com/jsf/facelets" xmlns:f="http://java.sun.com/jsf/core" xmlns:util="http://java.sun.com/jsf/composite/util">
<h:head>
	<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />

	<h:outputStylesheet library="css" name="styles.css" />
	<h:outputStylesheet library="css" name="grid.css" />
	<h:outputStylesheet library="css" name="bootstrap.css" />
	
	<h:outputScript name="utilities.js" library="javascript" target="head" />
	<script type="text/javascript">
		var mode;
		function getEmployee(contextPath) {
			mode = 2;
			openEmpsMiniSearch(contextPath, 2);
			return false;
		}

		function getAgent(contextPath) {
			mode = 1;
			openAssignmentsMiniSearch(contextPath,3);
			return false;
		}

		function releaseMask() {
			unMaskScreen();
		}

		function HandlePopupResult(ret) {
			unMaskScreen();
			if (ret != null) { 
				if (mode == 2) {
					document.getElementById('monthlyRewardSearchFormId:employeeSearchIdId').value = ret[0];
					document.getElementById('monthlyRewardSearchFormId:employeeSearchNameId').innerHTML = ret[1];
				} else if (mode == 1) {
					document.getElementById('monthlyRewardSearchFormId:agentSearchIdId').value=ret[0];
	         	    document.getElementById('monthlyRewardSearchFormId:agentSearchCodeId').innerHTML=ret[2];
					return true;
				}
			}
			return false;
		}

		window.onkeypress = function(e) {
			  var keyCode = window.event.keyCode || e.keyCode || e.which;
		      if (keyCode == '13') {
		    	  document.getElementById('monthlyRewardSearchFormId:searchBtnId').click();
		    	  return false;
		      }
		};
	</script>
</h:head>
<h:body styleClass="screenBg">
	<h:form id="monthlyRewardSearchFormId" dir="#{msgs.dir}">
		<ui:decorate template="/Main/Template.xhtml">
			<table border="0" cellpadding="0" cellspacing="0" width="100%">
				<tr>
					<td class="screenTitle">#{msgs.title_monthlyRewardSearch}</td>
				</tr>

				<tr>
					<td width="100%" height="5" />
				</tr>

				<tr>
					<td colspan="2">
						<table class="sectionTitle" border="0" cellpadding="0" cellspacing="0">
							<tr>
								<td>
									<h:outputText value="#{msgs.label_searchData}" />
								</td>
							</tr>
						</table>
					</td>
				</tr>

				<tr>
					<td width="100%" height="5" />
				</tr>

				<tr>
					<td>
						<h:panelGroup id="searchContainerId">
							<table class="sectionContent" border="0" cellpadding="0" cellspacing="0">
								<tr>
									<th width="15%">
										<h:outputText value="#{msgs.label_financialYear}" />
									</th>
									<td width="20%">
										<h:selectOneMenu styleClass="form-control" value="#{monthlyRewardSearch.finMonthlyRewardResourceDataSearch.year}">
											<f:selectItems value="#{monthlyRewardSearch.finYears}" var="y" itemLabel="#{y.intValue()}" itemValue="#{y.intValue()}" />
										</h:selectOneMenu>
									</td>
									

									<th width="15%">
										<h:outputText value="#{msgs.label_month}" />
									</th>
									<td width="20%">
										<h:selectOneMenu styleClass="form-control" value="#{monthlyRewardSearch.finMonthlyRewardResourceDataSearch.month}">
											<f:selectItem itemLabel="#{msgs.label_moharm}" itemValue="1" />
											<f:selectItem itemLabel="#{msgs.label_safr}" itemValue="2" />
											<f:selectItem itemLabel="#{msgs.label_rabieAwl}" itemValue="3" />
											<f:selectItem itemLabel="#{msgs.label_rabieTani}" itemValue="4" />
											<f:selectItem itemLabel="#{msgs.label_gamadAwl}" itemValue="5" />
											<f:selectItem itemLabel="#{msgs.label_gamadTani}" itemValue="6" />
											<f:selectItem itemLabel="#{msgs.label_ragb}" itemValue="7" />
											<f:selectItem itemLabel="#{msgs.label_shaban}" itemValue="8" />
											<f:selectItem itemLabel="#{msgs.label_ramdan}" itemValue="9" />
											<f:selectItem itemLabel="#{msgs.label_shwal}" itemValue="10" />
											<f:selectItem itemLabel="#{msgs.label_zwAlqada}" itemValue="11" />
											<f:selectItem itemLabel="#{msgs.label_zwAlhaga}" itemValue="12" />
										</h:selectOneMenu>
									</td>
									
								</tr>

								<tr>
									<th>
										<h:outputText value="#{msgs.label_assignmentCode}" />
										<h:commandButton value="&#xf002;" styleClass="btn btn-trans-icon" id="searchAgentBtnId" onclick="return getAgent('#{request.contextPath}');" />
									</th>
									<td>
										<h:inputHidden id="agentSearchIdId" value="#{monthlyRewardSearch.finMonthlyRewardResourceDataSearch.assignmentDetailId}" />
										<h:outputText id="agentSearchCodeId" value="#{monthlyRewardSearch.finMonthlyRewardResourceDataSearch.agentCode}" />
									</td>
									
									<th>
										<h:outputText value="#{msgs.label_assignmentOfficer}" />
										<h:commandButton value="&#xf002;" styleClass="btn btn-trans-icon" id="searchEmployeeBtnId" onclick="return getEmployee('#{request.contextPath}');" rendered="#{monthlyRewardSearch.pageMode==1}" />
									</th>
									<td>
										<h:inputHidden id="employeeSearchIdId" value="#{monthlyRewardSearch.finMonthlyRewardResourceDataSearch.agentOfficerId}" />
										<h:outputText id="employeeSearchNameId" value="#{monthlyRewardSearch.finMonthlyRewardResourceDataSearch.agentOfficerName}" />
									</td>
								
								</tr>

								<tr>
									<th>
										<h:outputText value="#{msgs.label_region}" />
									</th>
									<td>
										<h:selectOneMenu styleClass="form-control" value="#{monthlyRewardSearch.finMonthlyRewardResourceDataSearch.agentRegionId}" disabled="#{monthlyRewardSearch.pageMode==2}">
											<f:selectItem itemLabel="#{msgs.label_all}" itemValue="#{null}" />
											<f:selectItems value="#{monthlyRewardSearch.regionDepartments}" var="region" itemLabel="#{region.arabicName}" itemValue="#{region.id}" />
											<f:ajax event="change" render="monthlyRewardSearchFormId:agentSectorListId" execute="@this" listener="#{monthlyRewardSearch.getSectorsByRegionId()}" />
										</h:selectOneMenu>
									</td>
									
									<th>
										<h:outputText value="#{msgs.label_sectorUnit}" />
									</th>
									<td>
										<h:selectOneMenu id="agentSectorListId" styleClass="form-control" disabled="#{ empty monthlyRewardSearch.finMonthlyRewardResourceDataSearch.agentRegionId}" value="#{monthlyRewardSearch.finMonthlyRewardResourceDataSearch.agentSectorId}">
											<f:selectItem itemLabel="#{msgs.label_all}"  />
											<f:selectItems value="#{monthlyRewardSearch.sectorsDepartments}" var="sector" itemLabel="#{sector.arabicName}" itemValue="#{sector.id}" />
										</h:selectOneMenu>
									</td>
									
								</tr>
							</table>
						</h:panelGroup>
					</td>
				</tr>

				<tr>
					<td width="100%" height="5" />
				</tr>

				<tr>
					<td width="100%">
						<table class="actionsSection" border="0" cellpadding="0" cellspacing="0">
							<tr>
								<td>
									<h:commandButton styleClass="btn btn-default" id="resetBtnId" value="&#xf021; #{msgs.label_reset}" action="#{monthlyRewardSearch.resetSearch()}">
										<f:ajax render="monthlyRewardSearchFormId:searchContainerId" />
									</h:commandButton>

									<h:commandButton styleClass="btn btn-default" id="searchBtnId" value="&#xf002; #{msgs.label_search}" action="#{monthlyRewardSearch.search()}">
										<f:ajax render="monthlyRewardSearchFormId:monthlyRewardResultPaneId" execute="monthlyRewardSearchFormId:searchContainerId" />
									</h:commandButton>
								</td>
							</tr>
						</table>
					</td>
				</tr>

				<tr>
					<td width="100%" height="5" />
				</tr>

				<tr>
					<td>
						<table class="sectionTitle" border="0" cellpadding="0" cellspacing="0">
							<tr>
								<td>
									<h:outputText value="#{msgs.label_searchData}" />
								</td>
							</tr>
						</table>
					</td>
				</tr>

				<tr>
					<td width="100%">
						<table class="actionsSection" border="0" cellpadding="0" cellspacing="0">
						<tr>
					<td width="100%" height="10" />
				</tr>
							<tr>
								<td>
									<h:outputText value="#{msgs.label_all}" />
									<h:selectBooleanCheckbox>
										<f:ajax listener="#{monthlyRewardSearch.checkUncheckAllResources()}" execute="@this" render="monthlyRewardSearchFormId:monthlyRewardResultPaneId" />
									</h:selectBooleanCheckbox>
								</td>
							</tr>
						</table>
					</td>
				</tr>


				<tr>
					<td width="100%" height="5" />
				</tr>

				<tr>
					<td align="center" width="100%">
						<h:panelGroup id="monthlyRewardResultPaneId">
							<p:dataTable reflow="true" tableStyle="table-layout: auto" paginator="true" id="resourcesDataTableId" rows="10" rowIndexVar="index" var="row" value="#{monthlyRewardSearch.finMonthlyRewardResourceDataList}"
							rowHover="true">
								<p:column styleClass='centerText'>
									<f:facet name="header">#</f:facet>
	                                #{index + 1}
	                            </p:column>
								<p:column styleClass='centerText'>
									<f:facet name="header"> #{msgs.label_assignmentCode} </f:facet>
									<h:outputText value="#{row.agentCode}" />
								</p:column>
								<p:column styleClass='centerText'>
									<f:facet name="header"> #{msgs.label_name} </f:facet>
									<h:outputText value="#{row.agentName}" />
								</p:column>
								<p:column styleClass='centerText'>
									<f:facet name="header"> #{msgs.label_sectorUnit} </f:facet>
									<h:outputText value="#{row.agentSectorName}" />
								</p:column>
								<p:column styleClass='centerText'>
									<f:facet name="header"> #{msgs.label_amount} </f:facet>
									<h:outputText value="#{row.agentMonthlyReward}">
										<f:converter converterId="com.code.ui.converters.CSNumberConverter" />
									</h:outputText>
								</p:column>
								<p:column styleClass='centerText'>
									<f:facet name="header"> #{msgs.label_received} </f:facet>
									<h:selectBooleanCheckbox value="#{row.selected}" disabled="#{row.received}">
										<f:ajax listener="#{monthlyRewardSearch.raiseChangedFlag(row)}" />
									</h:selectBooleanCheckbox>
								</p:column>
							</p:dataTable>
						</h:panelGroup>

					</td>
				</tr>

				<tr>
					<td>
						<table class="actionsSection" border="0" cellpadding="0" cellspacing="0">
						<tr>
					<td width="100%" height="5" />
				</tr>
							<tr>
								<td width="100%">
									<h:commandButton styleClass="btn btn-success" value="&#xf00c; #{msgs.label_received}" action="#{monthlyRewardSearch.updateFinMonthlyRewardResource()}" onclick="return confirm('#{msgs.q_deliver}');">
										<f:ajax execute="monthlyRewardSearchFormId:monthlyRewardResultPaneId" render="monthlyRewardSearchFormId:monthlyRewardResultPaneId" />
									</h:commandButton>
								</td>
							</tr>
						</table>
					</td>
				</tr>
			</table>
		</ui:decorate>
	</h:form>
</h:body>
</html>