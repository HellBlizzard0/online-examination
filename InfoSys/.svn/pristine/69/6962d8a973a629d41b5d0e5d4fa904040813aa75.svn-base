	<queryString>
		<![CDATA[SELECT COUNT(LAB_CHECK_EMP.ID) "COUNT(LAB_CHECK_EMP.ID)",
CASE LAB_CHECK_EMP."CHECK_STATUS"
WHEN 1 THEN 'لم يتم أخذ العينة'
WHEN 2 THEN 'سلبي'
WHEN 3 THEN 'إيجابي تحت التأكيد'
WHEN 4 THEN 'تم إرسال لقوي الأمن'
WHEN 5 THEN 'إيجابي'
WHEN 6 THEN 'امتناع فحص'
WHEN 7 THEN'تم أخذ العينة'
WHEN 8 THEN 'إعادة الفحص'
WHEN 9 THEN 'غش العينة'
END AS CHECK_STATUS,
  REGION.ARABIC_NAME AS REGION_ARABIC_NAME
FROM $P!{P_SCHEMA_NAME}.FIS_VW_LAB_CHECK_EMPLOYEES LAB_CHECK_EMP
LEFT OUTER JOIN $P!{P_SCHEMA_NAME}.STP_VW_DEPARTMENTS REGION ON LAB_CHECK_EMP.EMPLOYEE_REGION_ID = REGION.ID
WHERE ($P{P_FROM_DATE_NULL} = 1 OR ORDER_DATE >= $P!{P_SCHEMA_NAME}.PKG_CHAR_TO_DATE($P{P_FROM_DATE} , 'MI/MM/YYYY'))
AND ($P{P_TO_DATE_NULL} = 1 OR ORDER_DATE <= $P!{P_SCHEMA_NAME}.PKG_CHAR_TO_DATE($P{P_TO_DATE} , 'MI/MM/YYYY'))
AND ( $P{P_LAB_CHECK_STATUS}         = -1
OR LAB_CHECK_EMP.CHECK_STATUS        = $P{P_LAB_CHECK_STATUS} )
AND ( $P{P_LAB_CHECK_REASON}         = -1
OR LAB_CHECK_EMP.CHECK_REASON        = $P{P_LAB_CHECK_REASON})
AND ( $P{P_REGION_ID}                = -1
OR LAB_CHECK_EMP.EMPLOYEE_REGION_ID  = $P{P_REGION_ID})
AND LAB_CHECK_EMP.EMPLOYEE_REGION_ID is not null
AND LAB_CHECK_EMP.CHECK_STATUS IS NOT NULL
GROUP BY REGION.ARABIC_NAME,
  LAB_CHECK_EMP.CHECK_STATUS
ORDER BY REGION.ARABIC_NAME,
  LAB_CHECK_EMP.CHECK_STATUS]]>
	</queryString>
