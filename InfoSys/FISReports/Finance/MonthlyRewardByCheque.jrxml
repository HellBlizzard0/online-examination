WHERE DTL.MONTHLY_REWARDS_ID = $P{P_MONTHLY_REWARD_ID}
AND DTL.AGENT_PAYMENT_METHOD = $P{P_PAYMENT_METHOD}

UNION

SELECT DTL.AGENT_OFFICER_NAME AS NAME , SUM(DTL.AGENT_MONTHLY_REWARD) AS AMOUNT
FROM $P!{P_SCHEMA_NAME}.FIS_VW_MONTHLY_REWRD_DEPT_DTLS DTL
WHERE DTL.MONTHLY_REWARDS_ID = $P{P_MONTHLY_REWARD_ID}
