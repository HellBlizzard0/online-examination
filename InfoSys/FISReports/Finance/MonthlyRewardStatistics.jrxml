	<queryString>
		<![CDATA[SELECT COUNT(DTL.ID) AS TOTAL_COUNT , SUM (DTL.AGENT_MONTHLY_REWARD) AS TOTAL_SUM , DTL.AGENT_REGION_NAME , $P!{P_SCHEMA_NAME}.PKG_SUBSTRING(DTL.AGENT_SECTOR_NAME, $P!{P_SCHEMA_NAME}.PKG_INSTR(DTL.AGENT_SECTOR_NAME, '/')+1, $P!{P_SCHEMA_NAME}.PKG_LENGTH(DTL.AGENT_SECTOR_NAME)) as AGENT_SECTOR_NAME ,DTL.MONTH_NUMBER
FROM $P!{P_SCHEMA_NAME}.FIS_VW_MONTHLY_REWRD_DEPT_DTLS DTL
WHERE ($P{P_REGION_ID} = -1 OR DTL.AGENT_REGION_ID = $P{P_REGION_ID} )
AND ($P{P_SECTOR_ID} = -1 OR DTL.AGENT_SECTOR_ID =  $P{P_SECTOR_ID})
AND ($P!{P_SCHEMA_NAME}.PKG_CHAR_TO_DATE ($P{P_FROM_DATE}, 'MI/MM/YYYY')  <= DTL.REQUEST_DATE)
AND ($P!{P_SCHEMA_NAME}.PKG_CHAR_TO_DATE ($P{P_TO_DATE}, 'MI/MM/YYYY')  >= DTL.REQUEST_DATE)
AND DTL.MONTHLY_REWARD_STATUS = $P{P_MONTHLY_REWARD_STATUS}
GROUP BY DTL.AGENT_REGION_ID , DTL.AGENT_REGION_NAME , DTL.MONTH_NUMBER , DTL.AGENT_SECTOR_NAME , DTL.AGENT_SECTOR_ID
order by DTL.MONTH_NUMBER,DTL.AGENT_REGION_ID,AGENT_SECTOR_NAME]]>
	</queryString>
