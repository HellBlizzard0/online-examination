	<queryString>
		<![CDATA[SELECT SUPP.SUPPORT_DATE,
       SUPP.REWARD_SUPPORT_BALANCE,
       SUPP.REGIONS_SUPPORT_BALANCE,
       SUPP.REGIONS_SUPPORT_BALANCE + SUPP.REWARD_SUPPORT_BALANCE AS BALANCE
FROM $P!{P_SCHEMA_NAME}.FIS_FIN_YEAR_APPROVALS ARV , $P!{P_SCHEMA_NAME}.FIS_FIN_YEAR_ACCOUNT_SUPPORT SUPP
WHERE ARV.ID = SUPP.FIN_YEAR_APPROVAL_ID
AND ARV.FIN_YEAR = $P{P_YEAR}
AND ($P!{P_SCHEMA_NAME}.PKG_CHAR_TO_DATE ($P{P_FROM_DATE}, 'MI/MM/YYYY')  <= SUPP.SUPPORT_DATE)
AND ($P!{P_SCHEMA_NAME}.PKG_CHAR_TO_DATE ($P{P_TO_DATE}, 'MI/MM/YYYY')  >= SUPP.SUPPORT_DATE)
ORDER BY SUPP.SUPPORT_DATE]]>
	</queryString>
