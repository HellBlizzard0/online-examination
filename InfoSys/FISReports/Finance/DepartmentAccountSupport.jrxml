	<queryString>
		<![CDATA[SELECT DS.REQUEST_NUMBER, $P!{P_SCHEMA_NAME}.PKG_DATE_TO_CHAR (DS.REQUEST_DATE,'YYYY/mm/MI') REQUEST_DATE,
DSD.SUPPORT_AMOUNT, DSD.DEPARTMENT_NAME,
$P!{P_SCHEMA_NAME}.PKG_SUBSTRING(DSD.DEPARTMENT_NAME, $P!{P_SCHEMA_NAME}.PKG_INSTR(DSD.DEPARTMENT_NAME, '/')+1, $P!{P_SCHEMA_NAME}.PKG_LENGTH(DSD.DEPARTMENT_NAME)) as DEP_NAME,
R.ARABIC_NAME REGION_NAME
FROM $P!{P_SCHEMA_NAME}.FIS_DEPARTMENTS_SUPPORTS DS, $P!{P_SCHEMA_NAME}.FIS_VW_DEPS_SUPPORT_DETAILS DSD, $P!{P_SCHEMA_NAME}.STP_VW_DEPARTMENTS D, $P!{P_SCHEMA_NAME}.STP_VW_DEPARTMENTS R,$P!{P_SCHEMA_NAME}.STP_VW_DEPARTMENTS_SECOTRS SECT
WHERE DS.ID = DSD.DEPARTMENTS_SUPPORTS_ID
AND D.ID = DSD.STP_VW_DEPARTMENTS_ID
AND SECT.ID = D.ID
AND D.REGIONID = R.ID
AND DS.APPROVED = 1
AND DS.REQUEST_DATE >= $P!{P_SCHEMA_NAME}.PKG_CHAR_TO_DATE ($P{P_START_DATE},'MI/mm/YYYY')
AND DS.REQUEST_DATE <= $P!{P_SCHEMA_NAME}.PKG_CHAR_TO_DATE ($P{P_END_DATE},'MI/mm/YYYY')
AND ($P{P_REGION_ID} = -1 OR D.REGIONID = $P{P_REGION_ID} OR D.ID = $P{P_REGION_ID})
AND ($P{P_SECTOR_ID} = -1 OR SECT.SECTORID = $P{P_SECTOR_ID} OR D.ID = $P{P_SECTOR_ID})
ORDER BY REGION_NAME, DEPARTMENT_NAME, REQUEST_DATE]]>
	</queryString>
