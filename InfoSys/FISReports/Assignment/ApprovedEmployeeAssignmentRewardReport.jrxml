	<queryString>
		<![CDATA[SELECT
  ad.FULL_NAME,
  e.SOCIAL_ID,
  e.MILITARY_NO,
  e.RANK,
  e.ACTUAL_DEPARTMENT_NAME

FROM $P!{P_SCHEMA_NAME}.FIS_VW_ASSIGNMENTS_DETAILS ad , $P!{P_SCHEMA_NAME}.STP_VW_EMPLOYEES e
WHERE
  ad.STP_VW_EMPLOYEES_ID = e.ID AND
  ad.ASSIGNMENTS_ID = $P{P_ASSIGNMENT_ID} AND
  ad.ELIMINATED = 0 AND
  ad.STATUS = 1]]>
	</queryString>
