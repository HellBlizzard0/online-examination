	<queryString>
		<![CDATA[SELECT AD.FULL_NAME AGENT_NAME, $P!{P_SCHEMA_NAME}.PKG_DATE_TO_CHAR(AD.START_DATE,'YYYY/mm/MI') START_DATE, $P!{P_SCHEMA_NAME}.PKG_DATE_TO_CHAR(AD.APPROVED_END_DATE,'YYYY/mm/MI') END_DATE, AD.AGENT_CODE,
A.STP_REGION_NAME REGION_NAME, D.LATIN_NAME SECTOR_NAME, A.STP_VW_EMPLOYEES_NAME_OFFICER OFFICER_NAME,
(SELECT COUNT(*)
FROM $P!{P_SCHEMA_NAME}.FIS_INFO_SOURCES S
WHERE S.ASSIGNMENT_DETAILS_ID = AD.ID ) INFO_COUNT,
AD.STATUS STATUS
FROM $P!{P_SCHEMA_NAME}.FIS_VW_ASSIGNMENTS A
JOIN $P!{P_SCHEMA_NAME}.FIS_ASSIGNMENTS_DETAILS AD ON  A.ID = AD.ASSIGNMENTS_ID
 LEFT OUTER JOIN $P!{P_SCHEMA_NAME}.STP_VW_DEPARTMENTS D ON A.STP_SECTOR_ID = D.ID
WHERE AD.STP_VW_EMPLOYEES_ID IS NOT NULL
AND ($P{P_REGION_ID} = -1 OR A.STP_REGION_ID = $P{P_REGION_ID})
AND ($P{P_SECTOR_ID} = -1 OR A.STP_SECTOR_ID = $P{P_SECTOR_ID})
AND ($P{P_OFFICER_ID} = -1 OR A.STP_VW_EMPLOYEES_ID_OFFICER = $P{P_OFFICER_ID})
AND ($P{P_ASSIGNMENT_STATUS} = -1 or $P{P_ASSIGNMENT_STATUS} = AD.STATUS)
AND AD.ELIMINATED = 0
AND ($P{P_ACTIVE} = -1 OR
    ( ($P{P_ACTIVE} = 1 OR AD.APPROVED_END_DATE <= $P!{P_SCHEMA_NAME}.PKG_CHAR_TO_DATE($P{P_HIJRI_SYS_DATE}, 'MI/MM/YYYY')) AND
    ($P{P_ACTIVE} = 0 OR AD.APPROVED_END_DATE > $P!{P_SCHEMA_NAME}.PKG_CHAR_TO_DATE($P{P_HIJRI_SYS_DATE}, 'MI/MM/YYYY'))))
AND AD.APPROVED_END_DATE >= $P!{P_SCHEMA_NAME}.PKG_CHAR_TO_DATE($P{P_START_DATE},'MI/mm/YYYY')
AND AD.START_DATE <= $P!{P_SCHEMA_NAME}.PKG_CHAR_TO_DATE($P{P_END_DATE},'MI/mm/YYYY')
ORDER BY REGION_NAME, SECTOR_NAME]]>
	</queryString>
