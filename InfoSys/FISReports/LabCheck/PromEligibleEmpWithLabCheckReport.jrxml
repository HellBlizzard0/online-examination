	<parameter name="P_LAB_CHECK_STATUS_NAME" class="java.lang.String"/>
	<parameter name="P_LAB_CHECK_RESULT_NAME" class="java.lang.String"/>
	<parameter name="P_PROMOTION_EXECUTION_DATE_BEFORE_YEAR" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="P_LAB_CHECK_RESULT_LIST" class="java.util.List"/>
	<queryString>
		<![CDATA[SELECT
stp_emp.*,
hcm_emp.official_unit_full_name,
hcm_emp.physical_unit_full_name,
TO_CHAR(hcm_emp.promotion_due_date, 'yyyy/MM/MI') AS promotion_due_date

FROM
hcm_vw_prs_emps hcm_emp,
(SELECT
    emps.id,
    emps.full_name,
    emps.social_id,
    emps.rank_id,
    last_lab_check.order_date,
    DECODE(last_lab_check.check_status, 1, 'لم يتم أخذ العينة', 2, 'سلبى', 3, 'أيجابى تحت التأكيد',4, 'أرسل لمستشفى قوى الأمن', 5, 'إيجابى'
            , 6, 'إمتنع عن الفحص', 7, 'تم أخذ العينة وبانتظار النتيجة', 8, 'إعادة الفحص', 9, 'غش عينة') AS status
FROM
    (
        SELECT
            stp_vw_employees_id,
	  check_status,
            MAX(TO_CHAR(order_date, 'yyyy/mm/MI')) AS order_date
        FROM
            fis_vw_lab_check_employees
        WHERE
            lab_check_status = 2
            AND ($P{P_RANK} = -1 OR employee_rank_id = $P{P_RANK})
            AND order_date >= TO_DATE($P{P_PROMOTION_EXECUTION_DATE_BEFORE_YEAR}, 'MI/mm/YYYY')
            AND order_date <= TO_DATE($P{P_PROMOTION_EXECUTION_DATE}, 'MI/mm/YYYY')
        GROUP BY
            stp_vw_employees_id,check_status
    ) last_lab_check,
	stp_vw_departments dep,
    stp_vw_employees emps
WHERE
    (($P{P_LAB_CHECK_RESULT} = -1 AND $X{ IN , last_lab_check.check_status , P_LAB_CHECK_RESULT_LIST} ) OR last_lab_check.check_status = $P{P_LAB_CHECK_RESULT})
    AND last_lab_check.stp_vw_employees_id = emps.id
    AND emps.id IN (
        SELECT
            id
        FROM
            hcm_vw_prs_emps
        WHERE
            category_id = $P{P_CATEGORY_ID}
            AND ($P{P_RANK} = -1 OR rank_id = $P{P_RANK})
            AND status_id <> 70
            AND promotion_due_date <= TO_DATE($P{P_PROMOTION_ELIGIBLE_LAST_DATE}, 'MI/MM/YYYY')
            AND service_termination_date IS NULL
    )
	AND ($P{P_RANK} = -1 OR emps.rank_id = $P{P_RANK})
	AND emps.actual_dept_id = dep.id
	AND ($P{P_REGION_ID} = -1 OR dep.regionid = $P{P_REGION_ID})) stp_emp

where stp_emp.id = hcm_emp.id AND ($P{P_RANK} = -1 OR stp_emp.rank_id = $P{P_RANK})]]>
	</queryString>
