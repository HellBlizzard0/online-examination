		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT
    pe.id,
    pe.name,
    pe.social_id,
    pe.official_unit_full_name,
    pe.physical_unit_full_name,
    TO_CHAR(pe.promotion_due_date,'yyyy/MM/MI') AS promotion_due_date,
    NULL AS order_date,
    NULL AS result
FROM
    hcm_vw_prs_emps pe
WHERE
    pe.category_id = $P{P_CATEGORY_ID}
    AND (
                $P{P_RANK} =-1
                OR    RANK_ID =$P{P_RANK}
            )
    AND   pe.status_id <> 70
    AND   pe.promotion_due_date <= TO_DATE($P{P_PROMOTION_ELIGIBLE_LAST_DATE},'MI/MM/YYYY')
    AND   pe.service_termination_date IS NULL
    AND ( $P{P_REGION_ID} =-1  OR    pe.STP_REGION_ID =$P{P_REGION_ID} )
    AND   pe.id NOT IN (
        SELECT
            stp_vw_employees_id
        FROM
            fis_vw_lab_check_employees
        WHERE
            lab_check_status = 2
            AND   order_date >= TO_DATE($P{P_PROMOTION_EXECUTION_DATE_BEFORE_YEAR},'MI/mm/YYYY')
            AND   order_date <= TO_DATE($P{P_PROMOTION_EXECUTION_DATE},'MI/mm/YYYY')
             AND (
                $P{P_RANK} =-1
                OR    EMPLOYEE_RANK_ID =$P{P_RANK}
            )

    )]]>
	</queryString>
