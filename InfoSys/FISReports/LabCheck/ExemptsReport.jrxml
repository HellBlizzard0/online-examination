	<queryString>
		<![CDATA[SELECT
  e.SOCIAL_ID,
  e.EMPLOYEE_NAME,
  e.OLD_RANK_DESC,
  e.OLD_UNIT_FULL_NAME,
  e.MEDICAL_TEST_EXEMPTION_REASON,
  e.EMPLOYEE_CUR_RANK,
  e.EMPLOYEE_CUR_PHYSICAL_UNIT
FROM
  $P!{P_SCHEMA_NAME}.HCM_INTEG_PRM_EXEMPTION e
WHERE
  e.PROMOTION_DATE   >= $P!{P_SCHEMA_NAME}.PKG_CHAR_TO_DATE($P{P_FROM_DATE},'MI/MM/YYYY')
AND e.PROMOTION_DATE <= $P!{P_SCHEMA_NAME}.PKG_CHAR_TO_DATE($P{P_TO_DATE},'MI/MM/YYYY')
AND
  (
    $P{P_REGION_ID}  = -1
  OR  ( CASE EMPLOYEE_CUR_PHYSICAL_REGION
WHEN 1  THEN 101010000
WHEN 2  THEN 101010000
WHEN 3  THEN 1000000002
WHEN 4  THEN 1000000004
WHEN 6  THEN 1000000006
WHEN 7  THEN 1000000008
WHEN 8  THEN 1000000010
WHEN 9  THEN 1000000016
WHEN 10 THEN 1000000012
WHEN 12 THEN 1000000014
WHEN 13 THEN 1000005083 END ) = $P{P_REGION_ID}
  )
order by   e.PROMOTION_DATE ASC]]>
	</queryString>
