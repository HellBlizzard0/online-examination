	<queryString>
		<![CDATA[SELECT COUNT(SOURCES.ID) "COUNT(SOURCES.ID)",SOURCES.REGION_NAME,SOURCES.SECTOR_NAME, SUBJECT.DESCRIPTION as SUBJECT_DESC, TYPE.DESCRIPTION as TYPE_DESC
FROM $P!{P_SCHEMA_NAME}.FIS_VW_INFO_ALL_SOURCE SOURCES LEFT OUTER JOIN $P!{P_SCHEMA_NAME}.FIS_INFO_SUBJECTS SUBJECT ON SOURCES.INFO_SUBJECTS_ID = SUBJECT.ID
LEFT OUTER JOIN $P!{P_SCHEMA_NAME}.FIS_INFO_TYPES TYPE ON SUBJECT.INFO_TYPES_ID = TYPE.ID
WHERE  ($P{P_REGION_ID}                  = -1
OR SOURCES.STP_VW_DEPARTMENTS_ID_REGION = $P{P_REGION_ID})
AND ( $P{P_SECTOR_ID}                   = -1
OR SOURCES.STP_VW_DEPARTMENTS_ID_SECTOR = $P{P_SECTOR_ID})
AND SOURCES.RECEIVE_DATE                       >= $P!{P_SCHEMA_NAME}.PKG_CHAR_TO_DATE($P{P_FROM_DATE} , 'MI/MM/YYYY')
AND SOURCES.RECEIVE_DATE                       <= $P!{P_SCHEMA_NAME}.PKG_CHAR_TO_DATE($P{P_TO_DATE} , 'MI/MM/YYYY')
AND ( $P{P_SUBJECT_ID} = -1 or SUBJECT.ID = $P{P_SUBJECT_ID})
AND ( $P{P_TYPE_ID} = -1 or TYPE.ID = $P{P_TYPE_ID})
Group by SOURCES.REGION_NAME, SOURCES.SECTOR_NAME, SUBJECT.DESCRIPTION, TYPE.DESCRIPTION
ORDER by SOURCES.REGION_NAME, SOURCES.SECTOR_NAME]]>
	</queryString>
