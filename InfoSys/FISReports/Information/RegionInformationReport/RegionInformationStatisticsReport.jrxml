	<queryString>
		<![CDATA[SELECT COUNT(SOURCES.ID) "COUNT(SOURCES.ID)",SOURCES.REGION_NAME,SOURCES.SECTOR_NAME
FROM $P!{P_SCHEMA_NAME}.FIS_VW_INFO_ALL_SOURCE SOURCES
WHERE ($P{P_REGION_ID}                  = -1
OR STP_VW_DEPARTMENTS_ID_REGION = $P{P_REGION_ID})
AND ( $P{P_SECTOR_ID}                   = -1
OR STP_VW_DEPARTMENTS_ID_SECTOR = $P{P_SECTOR_ID})
AND RECEIVE_DATE                       >= $P!{P_SCHEMA_NAME}.PKG_CHAR_TO_DATE($P{P_FROM_DATE} , 'MI/MM/YYYY')
AND RECEIVE_DATE                       <= $P!{P_SCHEMA_NAME}.PKG_CHAR_TO_DATE($P{P_TO_DATE} , 'MI/MM/YYYY')
AND ($P{P_AGENT_CODE} = '-1' OR (SOURCES.AGENT_CODE LIKE $P{P_AGENT_CODE} and SOURCES.AGENT_CODE is not null))
Group by SOURCES.REGION_NAME, SOURCES.SECTOR_NAME]]>
	</queryString>
	<field name="COUNT(SOURCES.ID)" class="java.lang.Integer"/>
	<field name="REGION_NAME" class="java.lang.String"/>
