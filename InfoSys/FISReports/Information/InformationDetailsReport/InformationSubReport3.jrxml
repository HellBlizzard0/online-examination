	<parameter name="P_INFO_ID" class="java.math.BigDecimal"/>
	<parameter name="P_SCHEMA_NAME" class="java.lang.String">
		<defaultValueExpression><![CDATA["INFOSYS"]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT CASE ENTITY.STP_VW_EMPLOYEES_ID WHEN NULL THEN NON_EMP.FULL_NAME ELSE EMP.FULL_NAME END AS NAME ,
       CASE ENTITY.STP_VW_EMPLOYEES_ID WHEN NULL THEN NON_EMP."IDENTITY" ELSE  $P!{P_SCHEMA_NAME}.PKG_CHAR_TO_NUMBER(EMP.SOCIAL_ID) END AS IDENTITY_NUMBER,
       CASE ENTITY.STP_VW_EMPLOYEES_ID WHEN NULL THEN 'غير منسوب
' ELSE 'موظف' END AS TYPE,
       EMP.RANK,
       EMP.ACTUAL_DEPARTMENT_NAME
FROM  $P!{P_SCHEMA_NAME}.FIS_INFO_RELATED_ENTITIES ENTITY
      LEFT OUTER JOIN $P!{P_SCHEMA_NAME}.STP_VW_EMPLOYEES EMP ON EMP.ID = ENTITY.STP_VW_EMPLOYEES_ID
      LEFT OUTER JOIN $P!{P_SCHEMA_NAME}.FIS_NON_EMPLOYEES NON_EMP ON NON_EMP.ID = ENTITY.NON_EMPLOYEES_ID
WHERE ENTITY.INFORMATION_ID = $P{P_INFO_ID}
AND ENTITY.DOMAINS_ID IS NULL
AND ENTITY.STP_VW_COUNTIRES_ID IS NULL
AND ENTITY.STP_VW_DEPARTMENTS_ID IS NULL]]>
	</queryString>
