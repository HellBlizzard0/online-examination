	<parameter name="P_SCHEMA_NAME" class="java.lang.String">
		<defaultValueExpression><![CDATA["INFOSYS"]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT
  AGENT.AGENT_CODE                                AS NAME ,
  CASE AGENT.SOURCE_TYPE WHEN 1 THEN 'مصدر' WHEN 3 THEN 'متعاون' END AS TYPE ,
  AGENT.SOURCE_TYPE                               AS SOURCE_TYPE
FROM
  $P!{P_SCHEMA_NAME}.FIS_VW_INFO_ASSIGNMENTS AGENT
WHERE
  AGENT.INFORMATION_ID = $P{P_INFO_ID}
UNION
SELECT
  OPENSOURCE.OPEN_SOURCE_NAME AS NAME ,
  'مصدر مفتوح'          AS TYPE ,
  OPENSOURCE.SOURCE_TYPE      AS SOURCE_TYPE
FROM
  $P!{P_SCHEMA_NAME}.FIS_VW_INFO_OPEN_SOURCES OPENSOURCE
WHERE
  OPENSOURCE.INFORMATION_ID = $P{P_INFO_ID}]]>
	</queryString>
