	<queryString>
		<![CDATA[SELECT SEC.REQUEST_NUMBER,
SEC.REQUEST_DATE,
DEP.ARABIC_NAME,
SEC.REASON,
CASE PER.NOTES_EXISTS
WHEN NULL THEN 'لا توجد ملاحظات'
WHEN 0 THEN 'لا توجد ملاحظات'
ELSE 'توجد ملاحظات'END as NOTES,
CASE SEC."CHECK_REASON"
WHEN 1 THEN 'التحاق بالخدمة / تعيين أو توظيف'
WHEN 2 THEN 'تقاعد أو فصل'
WHEN 3 THEN 'تعاقد'
WHEN 4 THEN 'طلب استقالة'
WHEN 5 THEN 'استبعاد'END AS CHECK_REASON
FROM $P!{P_SCHEMA_NAME}.FIS_SECURITY_CHECKS SEC , $P!{P_SCHEMA_NAME}.FIS_SECURITY_CHECKS_PERSONS PER , $P!{P_SCHEMA_NAME}.STP_VW_DEPARTMENTS DEP
WHERE SEC.ID = PER.SECURITY_CHECKS_ID
AND DEP.ID = SEC.STP_VW_DEPARTMENTS_ID_ORDR_SRC
AND SEC.STATUS = 5
AND PER.NON_EMPLOYEES_ID = $P{P_NON_EMPLOYEE_ID}]]>
	</queryString>
