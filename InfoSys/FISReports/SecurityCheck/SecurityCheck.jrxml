	<queryString>
		<![CDATA[(SELECT
($P!{P_SCHEMA_NAME}.PKG_CONCAT(c.DEPARTMENT_NAME, $P!{P_SCHEMA_NAME}.PKG_CONCAT('/',c.INCOMING_SIDE_NAME))) SIDE_NAME,
$P!{P_SCHEMA_NAME}.PKG_CONCAT($P!{P_SCHEMA_NAME}.PKG_DATE_TO_CHAR (c.REQUEST_DATE,'YYYY/mm/MI'),'هـ') REQUEST_DATE,
c.REASON,
ce.FULL_NAME,
ce.SOCIAL_ID,
ce.NOTES_EXISTS,
'منسوب' as TYPE,
(CASE
WHEN c."CHECK_REASON" = 1 THEN 'التحاق بالخدمة / تعيين أو توظيف'
WHEN c."CHECK_REASON" = 2 THEN  'تقاعد أو فصل'
WHEN c."CHECK_REASON" = 3 THEN  'تعاقد'
WHEN c."CHECK_REASON" = 4 THEN  'طلب استقالة'
WHEN c."CHECK_REASON" = 5 THEN  'استبعاد'
END) AS CHECK_REASON
FROM
$P!{P_SCHEMA_NAME}.FIS_VW_SECURITY_CHECK c ,
$P!{P_SCHEMA_NAME}.FIS_VW_SECURITY_CHECK_EMPLOYEE ce
WHERE
($P{P_NOTES_EXIST} = -1 or ce.NOTES_EXISTS = $P{P_NOTES_EXIST}) AND
($P{P_CHECK_REASON} = -1 or c.CHECK_REASON = $P{P_CHECK_REASON}) AND
($P{P_DEPARTMENT_SOURCE_ID} = -1 OR c.STP_VW_DEPARTMENTS_ID_ORDR_SRC = $P{P_DEPARTMENT_SOURCE_ID}) AND
($P{P_DOMAIN_INCOMING_SOURCE_ID} = -1 OR c.DOMAINS_ID_INCOMING_SIDES = $P{P_DOMAIN_INCOMING_SOURCE_ID}) AND
($P{P_FROM_DATE_NULL} = 1 or $P!{P_SCHEMA_NAME}.PKG_CHAR_TO_DATE ($P{P_FROM_DATE}, 'MI/MM/YYYY')  < c.REQUEST_DATE) AND
($P{P_TO_DATE_NULL} = 1 or $P!{P_SCHEMA_NAME}.PKG_CHAR_TO_DATE ($P{P_TO_DATE}, 'MI/MM/YYYY')  > c.REQUEST_DATE) AND
ce.SECURITY_CHECKS_ID=c.ID AND
($P{P_REGION_ID} = -1 or c.REGION_ID = $P{P_REGION_ID})
union
SELECT
($P!{P_SCHEMA_NAME}.PKG_CONCAT(c.DEPARTMENT_NAME ,$P!{P_SCHEMA_NAME}.PKG_CONCAT('/',c.INCOMING_SIDE_NAME))) SIDE_NAME ,
$P!{P_SCHEMA_NAME}.PKG_CONCAT($P!{P_SCHEMA_NAME}.PKG_DATE_TO_CHAR (c.REQUEST_DATE,'YYYY/mm/MI'),'هـ') REQUEST_DATE,
c.REASON,
cne.FULL_NAME,
$P!{P_SCHEMA_NAME}.PKG_NUMBER_TO_CHAR (cne.IDENTITY_DESC) AS SOCIAL_ID,
cne.NOTES_EXISTS,
'أخرى' as TYPE,
(CASE
WHEN c."CHECK_REASON" = 1 THEN 'التحاق بالخدمة / تعيين أو توظيف'
WHEN c."CHECK_REASON" = 2 THEN  'تقاعد أو فصل'
WHEN c."CHECK_REASON" = 3 THEN  'تعاقد'
WHEN c."CHECK_REASON" = 4 THEN  'طلب استقالة'
WHEN c."CHECK_REASON" = 5 THEN  'استبعاد'
END
 ) AS CHECK_REASON
FROM
$P!{P_SCHEMA_NAME}.FIS_VW_SECURITY_CHECK c ,
$P!{P_SCHEMA_NAME}.FIS_VW_SECURITY_CHECK_NON_EMP cne
WHERE
($P{P_REGION_ID} = -1 or c.REGION_ID = $P{P_REGION_ID}) AND
($P{P_CHECK_REASON} = -1 or c.CHECK_REASON = $P{P_CHECK_REASON}) AND
($P{P_NOTES_EXIST} = -1 or cne.NOTES_EXISTS = $P{P_NOTES_EXIST}) AND
($P{P_DEPARTMENT_SOURCE_ID} = -1 OR c.STP_VW_DEPARTMENTS_ID_ORDR_SRC = $P{P_DEPARTMENT_SOURCE_ID}) AND
($P{P_DOMAIN_INCOMING_SOURCE_ID} = -1 OR c.DOMAINS_ID_INCOMING_SIDES = $P{P_DOMAIN_INCOMING_SOURCE_ID}) AND
($P{P_FROM_DATE_NULL} = 1 or $P!{P_SCHEMA_NAME}.PKG_CHAR_TO_DATE ($P{P_FROM_DATE}, 'MI/MM/YYYY')  <= c.REQUEST_DATE) AND
($P{P_TO_DATE_NULL} = 1 or $P!{P_SCHEMA_NAME}.PKG_CHAR_TO_DATE ($P{P_TO_DATE}, 'MI/MM/YYYY')  >= c.REQUEST_DATE) AND
cne.SECURITY_CHECKS_ID=c.ID) order by SIDE_NAME , REQUEST_DATE]]>
	</queryString>
