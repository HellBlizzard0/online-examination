	<queryString>
		<![CDATA[select  $P!{P_SCHEMA_NAME}.PKG_CONCAT($P!{P_SCHEMA_NAME}.PKG_DATE_TO_CHAR (sc.REQUEST_DATE, 'YYYY/MM/MI'),'هـ') REQUEST_DATE, sc.REQUEST_NUMBER, sc.DEPARTMENT_NAME, sc.INCOMING_SIDE_NAME,sc.REQUEST_SOURCE,
(CASE
WHEN sc.INCOMING_DATE = null THEN ''
ELSE $P!{P_SCHEMA_NAME}.PKG_CONCAT($P!{P_SCHEMA_NAME}.PKG_DATE_TO_CHAR (sc.INCOMING_DATE, 'YYYY/MM/MI'),'هـ')
END
) INCOMING_DATE, sc.INCOMMING_NUMBER, sc.REASON
,
(CASE
WHEN sc."CHECK_REASON" = 1 THEN 'التحاق بالخدمة / تعيين أو توظيف'
WHEN sc."CHECK_REASON" = 2 THEN  'تقاعد أو فصل'
WHEN sc."CHECK_REASON" = 3 THEN  'تعاقد'
WHEN sc."CHECK_REASON" = 4 THEN  'طلب استقالة'
WHEN sc."CHECK_REASON" = 5 THEN  'استبعاد'
END) AS CHECK_REASON
from $P!{P_SCHEMA_NAME}.FIS_VW_SECURITY_CHECK sc
where sc.ID = $P{P_SECURITY_CHECK_ID}]]>
	</queryString>
