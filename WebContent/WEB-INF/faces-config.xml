<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/javaee"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
 <navigation-rule>
  <from-view-id>/index.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>login</from-outcome>
   <to-view-id>/WEB-INF/login.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>about</from-outcome>
   <to-view-id>/WEB-INF/about.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>zx</from-outcome>
   <to-view-id>/Register.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/WEB-INF/login.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>admin-main</from-outcome>
   <to-view-id>/WEB-INF/admin-main.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/resources/utils/MainTemplate.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>about</from-outcome>
   <to-view-id>/WEB-INF/about.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <application/>
</faces-config>
